[EMBED]
EMBED %ControlEventHandling
[INSTANCES]
WHEN '?OK'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
Put(Yasal)
[GROUP]
PRIORITY 5000
INSTANCE 4
[END]
[END]
WHEN '?Yas:DonemBasiAy'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
If Yas:DonemBasiAy=1 Then Yas:DonemSonuAy=12
Else Yas:DonemSonuAy=Yas:DonemBasiAy-1.; Display
[END]
[END]
WHEN '?Button3'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
Clear(Dem:Record); Set(DEM:keykod,DEM:keykod)
Loop
  Next(Demirbas); If ErrorCode() Then Break.
  DonemBasiYil=Date(Yas:DonemBasiAy,1,Year(DEM:AlimTarih));
  DonemSonuYil=Date(Yas:DonemBasiAy,1,Year(DEM:AlimTarih)+1)-1
  Tah:DemirbasKodu=DEM:DemirbasKodu; Access:Tahakkuk.Fetch(Tah:keykod)

  If (DonemBasiYil>DEM:AlimTarih) Then If ~Tah:BuYilBirikmisAmortisman[1]>0 Then DEM:DonemYili=Year(DEM:AlimTarih)-1.
     Dem:DonemAy=(12-Yas:DonemSonuAy)+Month(Dem:AlimTarih)
  Else If ~Tah:BuYilBirikmisAmortisman[1]>0 Then DEM:DonemYili=Year(DEM:AlimTarih).
      Dem:DonemAy=Month(Dem:AlimTarih)-Yas:DonemBasiAy+1.

  ?Button3{Prop:Text}=Clip(DEM:DemirbasKodu)&' '&Clip(DEM:DemirbasAdi);Display
  If Int(DEM:AmortismanEritmeYili)<DEM:AmortismanEritmeYili Then
    DEM:AmortismanEritmeYili=Int(DEM:AmortismanEritmeYili)+1.
   Put(Demirbas)
End

?Button3{Prop:Text}='Hepsine Uygulandý'; Display
[END]
[END]
[END]
