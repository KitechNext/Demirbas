ExcelMavi       ROUTINE
    IF ~Excel THEN EXIT.
    DO ExcelTumSatir; EXLClass.SetFont(,,16711680,FONT:Bold)      ! COLOR:Red 255, COLOR:Blue 16711680

ExcelKolonFormatla  ROUTINE
    IF ~Excel THEN EXIT.

    ExK#=1
    Loop ExQ#=1 TO RECORDS(RapQ)
        Get(RapQ,ExQ#)
        IF RapQ:Yaz='E'
            If (RapQ:Ntlk='D' OR RapQ:Ntlk='F') THEN EXLClass.SelectRange(0,ExK#,,,false); EXLClass.SetFormat(CellFormat:Numeric,2,true).
            If RapQ:Ntlk='T' THEN EXLClass.SelectRange(0,ExK#,,,false); EXLClass.SetFormat(CellFormat:Date,'dd','mm','yyyy','.').
            !OleObj{'Application.WorkBooks("'&TWrkbName&'").WorkSheets("'&TShtName&'").Cells('&XRow&','&ii&').EntireColumn.NumberFormat'}='#,##0.00'
            ExK#+=1
        END
    End

