   OleObj{'Application.WorkBooks("'&TWrkbName&'").WorkSheets("'&TShtName&'").Cells('&XRow&','&1&').EntireRow.Font.ColorIndex'}=3
   Satir=  'Demirbaþ Grup : '
   If BGrub='' Then Satir=Clip(Satir)&' Baþtan ->' Else Satir=Clip(Satir)&' '&BGrub&' ->'.
   If SGrub='' Then Satir=Clip(Satir)&' Sona' Else Satir=Clip(Satir)&' '&SGrub.; Do ExcelYaz

   OleObj{'Application.WorkBooks("'&TWrkbName&'").WorkSheets("'&TShtName&'").Cells('&XRow&','&1&').EntireRow.Font.Bold'}=True
   OleObj{'Application.WorkBooks("'&TWrkbName&'").WorkSheets("'&TShtName&'").Cells('&XRow&','&1&').EntireRow.Font.ColorIndex'}=3
   Satir=  'Demirbaþ Kod  : '
   If BDKod='' Then Satir=Clip(Satir)&' Baþtan ->' Else Satir=Clip(Satir)&' '&BDKod&' ->'.
   If SDKod='' Then Satir=Clip(Satir)&' Sona' Else Satir=Clip(Satir)&' '&SDKod.; Do ExcelYaz

   OleObj{'Application.WorkBooks("'&TWrkbName&'").WorkSheets("'&TShtName&'").Cells('&XRow&','&1&').EntireRow.Font.Bold'}=True
   OleObj{'Application.WorkBooks("'&TWrkbName&'").WorkSheets("'&TShtName&'").Cells('&XRow&','&1&').EntireRow.Font.ColorIndex'}=3
