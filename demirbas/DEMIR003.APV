    IF ~Excel THEN EXIT.
    EXLClass.SelectUsedRange(Glo:DetayBaslangic,1,,,false)
    EXLClass.AutoFit()
    EXLClass.SelectRange(1,1,,,true)
    EXLClass.Kill()

