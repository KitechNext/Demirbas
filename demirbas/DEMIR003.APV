OF 2
    FnExportFile=GLB:PersonelDocsFolder&'\DB_'&FORMAT(TODAY(),@d12)&'-'&FORMAT(CLOCK(),@t02)&'.txt'
    LOOP satir#=1 TO Loc:MaxSatir
        Loc:TextRow=''
        LOOP sutun#=1 TO Loc:MaxSutun
            IF sutun#<>1 THEN Loc:TextRow=Loc:TextRow&'<9>'.
            LTQ:Row=satir#; LTQ:Column=sutun#
            GET(Loc:TextQ,LTQ:Row,LTQ:Column)
            IF NOT ERRORCODE() THEN Loc:TextRow=Loc:TextRow&LTQ:Value.
        END
        IF vSTR.ReplaceInline(Loc:TextRow,'<9>','',False)<>'' THEN
            st.AddLine(satir#,CLIP(LEFT(FORMAT(satir#,@n_10)))&'<9>'&Loc:TextRow)
        END
    END
    st.Join('<13,10>')
    st.SaveFile(FnExportFile,False)
    st.FreeLines()
    MESSAGE('Rapor dosyanýz hazýrlandý.|'&FnExportFile,'Rapor Tamamlandý',ICON:Tick)
OF 0
    If PrinterName='NPQ' Then PrintDrv::Finalise.
End
