[EMBED]
EMBED %AfterWindowOpening
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
If DRP:Ntlk<>'F' Then Disable(?Drp:Formul);Disable(?DRP:Formul:Prompt).
Display
[END]
EMBED %ControlEventHandling
[INSTANCES]
WHEN '?Button4'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
  Case Dls:Cins
    Of 'STRING'
    OrOf 'CSTRING'
      DRP:Ntlk='S'; DRP:Uzunluk=Dls:Uzunluk; IF Dls:Uzunluk>=255 THEN DRP:Frml='@s255' ELSE DRP:Frml='@s'&Format(Dls:Uzunluk,@n03).
      IF DRP:FieldKod<>DLS:Ad THEN DRP:FieldKod=Clip(Drp:FieldKod)&Dls:Ad.
    Of 'DATE'
      DRP:Ntlk='T'; DRP:Uzunluk=10; DRP:Frml='@d6b'
      IF DRP:FieldKod<>DLS:Ad THEN DRP:FieldKod=Clip(Drp:FieldKod)&Dls:Ad.
    Else
      DRP:Ntlk='D'; DRP:Uzunluk=15; DRP:Frml='@n15.2'
      IF DRP:FieldKod<>DLS:Ad THEN DRP:FieldKod=Clip(Drp:FieldKod)&Dls:Ad.
  End
  Display
[END]
[END]
WHEN '?DRP:Ntlk'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
Case DRP:Ntlk
Of 'S'; Disable(?Drp:Formul);Disable(?DRP:Formul:Prompt)
Of 'D'; Disable(?Drp:Formul);Disable(?DRP:Formul:Prompt)
Of 'T'; Disable(?Drp:Formul);Disable(?DRP:Formul:Prompt)
Of 'F'; Enable(?Drp:Formul);Enable(?DRP:Formul:Prompt)
End
