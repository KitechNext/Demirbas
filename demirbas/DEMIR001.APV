        If ~Tah:YDSSabitKiymetDegeri[No-1]>0 Then Tah:EskiNetDeger[No]=Tah:YDOSabitKiymetDegeri[No]-Tah:YDOAmortismanDegeri[No]-KistSonYilGidereAtilan$
        Else
            If Yil=YtlYil Then Tah:EskiNetDeger[No]=Round(Tah:KalanNetDeger[No-1]/1000000,YuvOran)
            Else Tah:EskiNetDeger[No]=Tah:KalanNetDeger[No-1].
        End
        If Yil=YtlYil Then Tah:KistSonYilGidereAtilan[No]=((Tah:KistSonYilGidereAtilan[No-1]/1000000)*(DegerlemeOrani$))
        Else Tah:KistSonYilGidereAtilan[No]=(Tah:KistSonYilGidereAtilan[No-1]*(DegerlemeOrani$)).
        Tah:YeniNetDeger[NO]=Tah:YDSSabitKiymetDegeri[No]-(Tah:YDSAmortismanDegeri[No]+Tah:KistSonYilGidereAtilan[No])
    Else
        Tah:EskiNetDeger[No]=Tah:YDOSabitKiymetDegeri[No]-Tah:YDOAmortismanDegeri[No]
        Tah:YeniNetDeger[NO]=Tah:YDSSabitKiymetDegeri[No]-Tah:YDSAmortismanDegeri[No]
    End

    IF DEG:HesapSekli='D' Then Tah:DegerlemeFonu[No]=Tah:YeniNetDeger[NO]-Tah:EskiNetDeger[No] Else Tah:DegerlemeFonu[No]=0.
    If Yil>=2004 And Tah:DuzeltmeyeEsasTutar+Tah:DuzeltilmisBirikmisAmortisman>0 Then Tah:DegerlemeFonu[No]=0.
    Tah:SabitKiymetNetArtisi[No]=Tah:YDSSabitKiymetDegeri[No]-Tah:YDOSabitKiymetDegeri[No]

    If ~DEM:AmortismanAyirma
       Case DEM:HesapSekli
       Of 'N'
          IF DEM:KistHesapSekli='U' Then Tah:BuYilAyrilanAmortisman[No]=Round(((Tah:YDSSabitKiymetDegeri[No]*(AmortismanAyirmaOrani$/100))/12),YuvOran)
          Else Tah:BuYilAyrilanAmortisman[No]=Round(((Tah:YDSSabitKiymetDegeri[No]*(AmortismanAyirmaOrani$/100))/12)*Ay,YuvOran).

       Of 'H'
           IF DEM:KistHesapSekli='U' Then Tah:BuYilAyrilanAmortisman[No]=Round(((Tah:YeniNetDeger[No]*(AmortismanAyirmaOrani$/100))/12),YuvOran)
           Else Tah:BuYilAyrilanAmortisman[No]=Round(((Tah:YeniNetDeger[No]*(AmortismanAyirmaOrani$/100))/12)*Ay,YuvOran).
       End
    Else Tah:BuYilAyrilanAmortisman[No]=0.
    Tah:BuYilBirikmisAmortisman[No]=Tah:BuYilAyrilanAmortisman[No]+Tah:YDSAmortismanDegeri[No]


    IF DEM:KistHesapSekli='O'
        Tah:KalanNetDeger[No]=Tah:YDSSabitKiymetDegeri[No]-Tah:BuYilBirikmisAmortisman[No]
    Else Tah:KalanNetDeger[No]=Tah:YDSSabitKiymetDegeri[No]-Tah:BuYilBirikmisAmortisman[No].

    If DEM:KistHesapSekli='P' And DEG:HesapSekli='E' And Yil=>2004 Then
        If Yil=2004 Then Tah:KistSonYilGidereAtilan[No]=Tah:DuzKistSonYilGidereAtilan*DegerlemeOrani$
        Else Tah:KistSonYilGidereAtilan[No]=Tah:KistSonYilGidereAtilan[No-1]*DegerlemeOrani$.
    Else Tah:KistSonYilGidereAtilan[No]=Tah:KistSonYilGidereAtilan[No].


    If (No=AmortismanEritmeYili$ AND ~DEM:AmortismanAyirma) Or (Dem:SifirlamaYili=Yil AND DEM:SifirlamaAyi<=Ay) Then  !Son Y˝l ›se
        IF DEM:KistHesapSekli='U'
           If Dem:DonemAy=1 And Ay=12 Or (Dem:SifirlamaYili=Yil AND DEM:SifirlamaAyi<=Ay) Then Tah:BuYilAyrilanAmortisman[No]+=Tah:KalanNetDeger[No]; Tah:KalanNetDeger[No]=0.
           If Dem:DonemAy>1 And Ay=Dem:DonemAy-1 Then Tah:BuYilAyrilanAmortisman[No]+=Tah:KalanNetDeger[No]; Tah:KalanNetDeger[No]=0.
        Else
            IF (DEM:KistHesapSekli='O' Or DEM:KistHesapSekli='P')
                If Ay=12 Or (Dem:SifirlamaYili=Yil AND DEM:SifirlamaAyi<=Ay) Then Tah:BuYilAyrilanAmortisman[No]+=Tah:KalanNetDeger[No]+KistSonYilGidereAtilan$
                Else Tah:BuYilAyrilanAmortisman[No]=((Tah:BuYilAyrilanAmortisman[No]+Tah:KalanNetDeger[No]+KistSonYilGidereAtilan$)/12)*Ay.
            Else
                If Ay=12 Or (Dem:SifirlamaYili=Yil AND DEM:SifirlamaAyi<=Ay) Then Tah:BuYilAyrilanAmortisman[No]+=Tah:KalanNetDeger[No]
                Else Tah:BuYilAyrilanAmortisman[No]=((Tah:BuYilAyrilanAmortisman[No]+Tah:KalanNetDeger[No])/12)*Ay.
            End
            Tah:BuYilAyrilanAmortisman[No]=Round(Tah:BuYilAyrilanAmortisman[No],YuvOran)
      !            If DEM:AmortismanAyirma Then Tah:BuYilAyrilanAmortisman[No]=0.

            If (DEM:KistHesapSekli='P') Then
                Tah:BuYilBirikmisAmortisman[No]=(Tah:BuYilAyrilanAmortisman[No]+Tah:YDSAmortismanDegeri[No])-KistSonYilGidereAtilan$
                If Ay=12 Or (Dem:SifirlamaYili=Yil AND DEM:SifirlamaAyi<=Ay) Then Tah:KalanNetDeger[No]=0
                Else Tah:KalanNetDeger[No]=Tah:YDSSabitKiymetDegeri[No]-(Tah:BuYilBirikmisAmortisman[No]+KistSonYilGidereAtilan$).
            Else
                Tah:BuYilBirikmisAmortisman[No]=Tah:BuYilAyrilanAmortisman[No]+Tah:YDSAmortismanDegeri[No]
                If Ay=12 Or (Dem:SifirlamaYili=Yil AND DEM:SifirlamaAyi<=Ay) Then Tah:KalanNetDeger[No]=0 Else Tah:KalanNetDeger[No]=Tah:YDSSabitKiymetDegeri[No]-Tah:BuYilBirikmisAmortisman[No].
            End
        End
    End

[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
OBirinciYilHesap Routine
  If Ay>=DEM:DonemAy
     If Yil>=2005
        DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil); DegerlemeOrani$=1; TefeYap$=False
        If DegYap='E' And ~DEM:Degerleme Then
          If DEG:HesapSekli='E' Then DegerlemeOrani$=1+DEG:TefeOrani[Ay]; TefeYap$=True.
        Else Clear(Deg:Record).
     Else
        DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil); DegerlemeOrani$=1; TefeYap$=False
        If DegYap='E' And ~DEM:Degerleme
        !÷nceki Y˝l Tefe Bilgisi Al
          DEG:DegerlemeYili=DEG:OncekiDonemHesaplananYil; Access:Degerleme.Fetch(DEG:KeyYil)
          If ErrorCode() Then Clear(Deg:Record); Message(Yil&' Y˝l˝ Deerleme Bilgileri Yok!','Demirba˛ Hesab˝',ICON:Exclamation).
          Tefe2$=DEG:TefeOrani[DEG:OncekiDonemHesaplananAy];

          !Bu Y˝l Tefe & DEgerleme Oran Bilgisi Al
          DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil)
          If ErrorCode() Then Clear(Deg:Record); Message(Yil&' Y˝l˝ Deerleme Bilgileri Yok!','Demirba˛ Hesab˝',ICON:Exclamation).
          Tefe1$=DEG:TefeOrani[Ay];

          If DEG:HesapSekli='E' And Yil=>2004 Then DegerlemeOrani$=(Tefe1$/Tefe2$); DegerlemeOrani$=Round(DegerlemeOrani$,.00001); TefeYap$=True
          Else DegerlemeOrani$=1.
        Else Clear(Deg:Record).
     End
    AmortismanAyirmaOrani$=DEM:AmortismanAyirmaOrani[1] !.
    AmortismanEritmeYili$=DEM:AmortismanEritmeYili

    If DEM:HesapSekli='H' !%50 GeÁmamali
       If AmortismanAyirmaOrani$>50 Then AmortismanAyirmaOrani$=50.
    End

    Case DEM:AmortismanAyirmaSekli
    Of 'D'; OrOf 'A'; OrOf 'B';
      If TefeYap$
        Tah:YDOSabitKiymetDegeri[1]=OTah:YDSSabitKiymetDegeri[1]
        Tah:YDOAmortismanDegeri[1]=OTah:YDSAmortismanDegeri[1]

        If Tah:YDOAmortismanDegeri[1]=0 Then Tah:YDOAmortismanDegeri[1]=OTah:BuYilAyrilanAmortisman[1].
        Tah:YDSSabitKiymetDegeri[1]=Tah:YDOSabitKiymetDegeri[1]*DegerlemeOrani$
        Tah:YDSAmortismanDegeri[1]=Tah:YDOAmortismanDegeri[1]*DegerlemeOrani$

        Tah:EskiNetDeger[1]=Tah:YDOSabitKiymetDegeri[1]
        Tah:YeniNetDeger[1]=Tah:YDSSabitKiymetDegeri[1]
        Tah:DegerlemeFonu[No]=0
        Tah:SabitKiymetNetArtisi[1]=Tah:YDSSabitKiymetDegeri[1]-Tah:YDOSabitKiymetDegeri[1]
        ! Esat
        If ~DEM:AmortismanAyirma
              Case DEM:HesapSekli
              Of 'N';Tah:BuYilAyrilanAmortisman[1]=Round(((Tah:YDSSabitKiymetDegeri[1]*(AmortismanAyirmaOrani$/100))/12)*Ay,YuvOran)
              Of 'H';Tah:BuYilAyrilanAmortisman[1]=Round(((Tah:YeniNetDeger[1]*(AmortismanAyirmaOrani$/100))/12)*Ay,YuvOran)
              End
        Else Tah:BuYilAyrilanAmortisman[1]=0.

      Else
        If ~DEM:AmortismanAyirma Then Tah:BuYilAyrilanAmortisman[1]=Round(((DEM:Tutar*(AmortismanAyirmaOrani$/100))/12)*Ay,YuvOran)
        Else Tah:BuYilAyrilanAmortisman[1]=0.
      End
    Of 'T' !Kist Ay Bulunuyor.

      If TefeYap$=True
        Tah:YDOSabitKiymetDegeri[1]=OTah:YDSSabitKiymetDegeri[1]
        Tah:YDOAmortismanDegeri[1]=OTah:YDSAmortismanDegeri[1]

        If Tah:YDOAmortismanDegeri[1]=0 Then Tah:YDOAmortismanDegeri[1]=OTah:BuYilAyrilanAmortisman[1].
        Tah:YDSSabitKiymetDegeri[1]=Tah:YDOSabitKiymetDegeri[1]*DegerlemeOrani$
        Tah:YDSAmortismanDegeri[1]=Tah:YDOAmortismanDegeri[1]*DegerlemeOrani$


        Tah:EskiNetDeger[1]=Tah:YDOSabitKiymetDegeri[1]
        Tah:YeniNetDeger[1]=Tah:YDSSabitKiymetDegeri[1]
        Tah:DegerlemeFonu[No]=0
        Tah:SabitKiymetNetArtisi[1]=Tah:YDSSabitKiymetDegeri[1]-Tah:YDOSabitKiymetDegeri[1]

        If Month(DEM:AlimTarih) <= Yas:DonemBasiAy Then KistAy=(Yas:DonemSonuAy-Month(DEM:AlimTarih))+1 Else KistAy=12-(Month(DEM:AlimTarih)-Yas:DonemBasiAy).
        If ~DEM:AmortismanAyirma
          Case DEM:KistHesapSekli
          Of 'Y'; Tah:BuYilAyrilanAmortisman[1]=Round(((((Tah:YDSSabitKiymetDegeri[1]/12)*KistAy)*(AmortismanAyirmaOrani$/100))/12)*((Ay-DEM:DonemAy)+1),YuvOran)
          Of 'A'; OrOf 'O'; OrOf 'P'; Tah:BuYilAyrilanAmortisman[1]=Round(((Tah:YDSSabitKiymetDegeri[1]*(AmortismanAyirmaOrani$/100))/12)*((Ay-DEM:DonemAy)+1),YuvOran)
          Of 'U'; Tah:BuYilAyrilanAmortisman[1]=Round(((Tah:YDSSabitKiymetDegeri[1]*(AmortismanAyirmaOrani$/100))/12),YuvOran)
          End
        Else Tah:BuYilAyrilanAmortisman[1]=0.

        If DEM:KistHesapSekli='U' Then Tah:KistSonYilGidereAtilan[1]=0
        Else Tah:KistSonYilGidereAtilan[1]=(Tah:YDSSabitKiymetDegeri[1]*(AmortismanAyirmaOrani$/100))-Tah:BuYilBirikmisAmortisman[1].
      Else
        If Month(DEM:AlimTarih) <= Yas:DonemBasiAy Then KistAy=(Yas:DonemSonuAy-Month(DEM:AlimTarih))+1 Else KistAy=12-(Month(DEM:AlimTarih)-Yas:DonemBasiAy).
        If ~DEM:AmortismanAyirma
          Case DEM:KistHesapSekli
          Of 'Y'; Tah:BuYilAyrilanAmortisman[1]=Round(((((DEM:Tutar/12)*KistAy)*(AmortismanAyirmaOrani$/100))/12)*((Ay-DEM:DonemAy)+1),YuvOran)
          Of 'A'; OrOf 'O'; OrOf 'P'; Tah:BuYilAyrilanAmortisman[1]=Round(((Dem:Tutar*(AmortismanAyirmaOrani$/100))/12)*((Ay-DEM:DonemAy)+1),YuvOran)
          End
        Else Tah:BuYilAyrilanAmortisman[1]=0.
        Tah:KistSonYilGidereAtilan[1]=(Dem:Tutar*(AmortismanAyirmaOrani$/100))-Tah:BuYilAyrilanAmortisman[1]
      End
    End ! Case

    Tah:Miktar[1]=DEM:KalanMiktar
    If DEM:AmortismanAyirmaSekli='T'
      Case DEM:KistHesapSekli
      Of 'P'; Tah:BuYilBirikmisAmortisman[1]=Tah:BuYilAyrilanAmortisman[1]+Tah:KistSonYilGidereAtilan[1]
      Of 'U'; Tah:BuYilBirikmisAmortisman[1]=Tah:BuYilAyrilanAmortisman[1]+(OTah:BuYilBirikmisAmortisman[1]*DegerlemeOrani$)
      Else Tah:BuYilBirikmisAmortisman[1]=Tah:BuYilAyrilanAmortisman[1].
    Else Tah:BuYilBirikmisAmortisman[1]=Tah:BuYilAyrilanAmortisman[1].

    If Tefeyap$ !Yil>=2004 And Year(DEM:AlimTarih)=>2004
      If DEM:AmortismanAyirmaSekli='T' And (DEM:KistHesapSekli='O' Or DEM:KistHesapSekli='P')
        If DEM:KistHesapSekli='O' Then Tah:KalanNetDeger[1]=Tah:YeniNetDeger[1]-(Tah:BuYilBirikmisAmortisman[1]+Tah:KistSonYilGidereAtilan[1]).
        If DEM:KistHesapSekli='P' Then Tah:KalanNetDeger[1]=Tah:YeniNetDeger[1]-(Tah:BuYilBirikmisAmortisman[1]).
      Else Tah:KalanNetDeger[1]=Tah:YeniNetDeger[1]-Tah:BuYilBirikmisAmortisman[1].
    Else
      If DEM:AmortismanAyirmaSekli='T' And (DEM:KistHesapSekli='O' Or DEM:KistHesapSekli='P')
        If DEM:KistHesapSekli='O' Then Tah:KalanNetDeger[1]=DEM:Tutar-(Tah:BuYilBirikmisAmortisman[1]+Tah:KistSonYilGidereAtilan[1]).
        If DEM:KistHesapSekli='P' Then Tah:KalanNetDeger[1]=DEM:Tutar-(Tah:BuYilBirikmisAmortisman[1]).
      Else Tah:KalanNetDeger[1]=DEM:Tutar-Tah:BuYilBirikmisAmortisman[1].
    End
    !Sat˝lan Demirba˛a Gˆre Hesapla
    If OTah:Miktar[1]<>DEM:KalanMiktar
      Tah:BuYilBirikmisAmortisman[1]=Round((Tah:BuYilBirikmisAmortisman[1]/OTah:Miktar[1])*DEM:KalanMiktar,YuvOran)
      If DEM:AmortismanAyirmaSekli='T' And (DEM:KistHesapSekli='O' Or DEM:KistHesapSekli='P')
        If DEM:KistHesapSekli='O' Then Tah:KalanNetDeger[1]=Round(((Tah:KalanNetDeger[1]-Tah:KistSonYilGidereAtilan[1])/OTah:Miktar[1])*DEM:KalanMiktar,YuvOran).
        If DEM:KistHesapSekli='P' Then Tah:KalanNetDeger[1]=Round(((Tah:KalanNetDeger[1])/OTah:Miktar[1])*DEM:KalanMiktar,YuvOran).
      Else
        Tah:KalanNetDeger[1]=Round((Tah:KalanNetDeger[1]/OTah:Miktar[1])*DEM:KalanMiktar,YuvOran)
      End
      If TefeYap$
        Tah:YDOSabitKiymetDegeri[1]=Round((Tah:YDOSabitKiymetDegeri[1]/OTah:Miktar[1])*DEM:KalanMiktar,YuvOran)
        Tah:YDSSabitKiymetDegeri[1]=Round((Tah:YDSSabitKiymetDegeri[1]/OTah:Miktar[1])*DEM:KalanMiktar,YuvOran)
        Tah:EskiNetDeger[1]=Tah:YDOSabitKiymetDegeri[1]
        Tah:YeniNetDeger[1]=Tah:YDSSabitKiymetDegeri[1]
      End
    End

    Tah:YDOSabitKiymetDegeri[No]=Round(Tah:YDOSabitKiymetDegeri[1],YuvOran)
    Tah:YDOAmortismanDegeri[No]=Round(Tah:YDOAmortismanDegeri[1],YuvOran)
    Tah:YDSSabitKiymetDegeri[No]=Round(Tah:YDSSabitKiymetDegeri[1],YuvOran)
    Tah:YDSAmortismanDegeri[No]=Round(Tah:YDSAmortismanDegeri[1],YuvOran)
    Tah:AmortismanFarki[No]=Round(Tah:AmortismanFarki[1],YuvOran)
    Tah:EskiNetDeger[No]=Round(Tah:EskiNetDeger[1],YuvOran)
    Tah:YeniNetDeger[NO]=Round(Tah:YeniNetDeger[1],YuvOran)
    Tah:DegerlemeFonu[No]=Round(Tah:DegerlemeFonu[1],YuvOran)
    Tah:SabitKiymetNetArtisi[No]=Round(Tah:SabitKiymetNetArtisi[1],YuvOran)
    Tah:BuYilAyrilanAmortisman[No]=Round(Tah:BuYilAyrilanAmortisman[1],YuvOran)
    Tah:BuYilBirikmisAmortisman[No]=Round(Tah:BuYilBirikmisAmortisman[1],YuvOran)
    Tah:KalanNetDeger[No]=Round(Tah:KalanNetDeger[1],YuvOran)
    If GidereAtilmayan Then Tah:BuYilGidereAtilmayanAmortisman[No]=Tah:BuYilAyrilanAmortisman[1] Else Tah:BuYilGidereAtilmayanAmortisman[1]=0.

    Put(Tahakkuk)
    ! Tahakkuk Dosyas˝ndan Degerlemefonu lar˝n˝ topla Demirbas Yaz
    DEM:BirikmisDegerlemeFonu=0; Loop i#=1 To 50; DEM:BirikmisDegerlemeFonu+=Tah:DegerlemeFonu[i#].;
    DEM:BirikmisDegerlemeFonu-=DEM:SermayeAktarilanFon; Put(Demirbas)
  End
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
OHesapla Routine
    Loop
        If Yil>DEM:DonemYili Then No=(Yil-DEM:DonemYili)+1
            If Yil>=2005
               DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil); DegerlemeOrani$=1; TefeYap$=False
               If DegYap='E' And ~DEM:Degerleme Then
                    CASE DEG:HesapSekli
                    OF 'E'
                        DegerlemeOrani$=1+DEG:TefeOrani[Ay]; TefeYap$=True
                    OF 'H'  ! Sﬁ 17.11.2021
                        IF DEM:AlimTarih<=DATE(06,09,2021) THEN  ! Yasa gerei 9 Haziran 2021 tarihi itibar˝yla al˝m˝ yap˝lm˝˛ demirba˛lar iÁin hesaplan˝r.
                            ! Demirba˛˝n ba˛lang˝Á oran˝n˝ al
                            IF DEM:DonemYili>=2005 THEN
                                IF DEM:DonemAy=12 THEN
                                    DEG:DegerlemeYili=DEM:DonemYili+1; Access:Degerleme.Fetch(DEG:KeyYil); Baslangic$=DEG:KatsayiOrani[1]
                                ELSE
                                    DEG:DegerlemeYili=DEM:DonemYili; Access:Degerleme.Fetch(DEG:KeyYil); Baslangic$=DEG:KatsayiOrani[DEM:DonemAy+1]
                                END
                            ELSE
                                DEG:DegerlemeYili='2005'; Access:Degerleme.Fetch(DEG:KeyYil); Baslangic$=DEG:KatsayiOrani[1]
                            END
                            DegerlemeOrani$=(Bitis$/Baslangic$)
                            DEM:HareketliDegerlemeOrani=DegerlemeOrani$
                            DEM:HareketliDegerlemeTarihi=DATE(Ay,1,Yil)

                            ! Hesaplama sonras˝ deerleme bilgilerini geri getir
                            DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil)
                        ELSE
                            DegerlemeOrani$=1
                        END
                        TefeYap$=True
                    OF 'D' ! Sﬁ 20.10.2021
                        DegerlemeOrani$=1+(DEG:DegerlemeOrani/100)
                    END
               Else Clear(Deg:Record).
            Else
               DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil); DegerlemeOrani$=1
               If DegYap='E' And ~DEM:Degerleme
                 !÷nceki Y˝l Tefe Bilgisi Al
                 DEG:DegerlemeYili=DEG:OncekiDonemHesaplananYil; Access:Degerleme.Fetch(DEG:KeyYil)
                 If ErrorCode() Then Clear(Deg:Record); Message(Yil&' Y˝l˝ Deerleme Bilgileri Yok!','Demirba˛ Hesab˝',ICON:Exclamation).
                 Tefe2$=DEG:TefeOrani[DEG:OncekiDonemHesaplananAy];

                 !Bu Y˝l Tefe & DEgerleme Oran Bilgisi Al
                 DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil)
                 If ErrorCode() Then Clear(Deg:Record); Message(Yil&' Y˝l˝ Deerleme Bilgileri Yok!','Demirba˛ Hesab˝',ICON:Exclamation).
                 Tefe1$=DEG:TefeOrani[Ay];

                 If DEG:HesapSekli='E' And Yil=>2004 Then DegerlemeOrani$=(Tefe1$/Tefe2$); DegerlemeOrani$=Round(DegerlemeOrani$,.00001)
                 Else DegerlemeOrani$=1+(DEG:DegerlemeOrani/100).
               Else Clear(Deg:Record).
            End

            If Tah:BuYilAyrilanAmortisman[No-1]<=0 And (DEM:AmortismanAyirma And Tah:KalanNetDeger[No-1]<=0)
              If Toplu<>'E' Then Message(DEM:DemirbasKodu&' ÷nceki Dˆnem Hesaplanmal˝d˝r.!','Demirba˛ Hesab˝',ICON:Exclamation).
            Else
                If Tah:KalanNetDeger[No-1]=0 Then
                  If DEG:KalanSifirEnfDevam And (DEG:HesapSekli='E' OR DEG:HesapSekli='H') And Yil=>2004 Then
                    Tah:YDOSabitKiymetDegeri[No]=Round(OTah:YDSSabitKiymetDegeri[No],.01)
                    Tah:YDOAmortismanDegeri[No]=Round(OTah:BuYilBirikmisAmortisman[No],.01)
                    Tah:YDSSabitKiymetDegeri[No]=Round((Tah:YDOSabitKiymetDegeri[No]*DegerlemeOrani$),.01)
                    Tah:YDSAmortismanDegeri[No]=Round((Tah:YDOAmortismanDegeri[No]*DegerlemeOrani$),.01)
