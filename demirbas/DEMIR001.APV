EMBED %ControlEventHandling
[INSTANCES]
WHEN '?Button2'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
IF Yil>=DEM:DonemYili
    DEG:DegerlemeYili=Yil
    If Access:Degerleme.Fetch(DEG:KeyYil)<>Level:Benign OR DEG:HesapSekli='' Then Message(Yil&' y˝l˝ iÁin amortisman deerleme yˆntemini seÁiniz.','Amortisman Hesab˝',ICON:Exclamation); Cycle.
    DemHesabi('H',Dem:DemirbasKodu,'','','','','',Yil,Ay,DegerlemeYapilsinmi,'','',Cnn,ODnmHspl,YtlYil,Toyotetsu,BinaSonYilYok,GidereAtilmasin,Loc:BitmisleriDegerle+Loc:BinalaridaDegerle)
    DovizDemHesabi('H',Dem:DemirbasKodu,'','','','','',Yil,Ay)
    Access:Demirbas.Fetch(DEM:keykod); Yil+=1
ELSE
    Message('Hesaplama y˝l˝ Al˝m Tarihinden k¸Á¸k olamaz.','Amortisman Hesab˝',ICON:Exclamation);
    Yil=DEM:DonemYili
END
IF Yil>Yas:CalisilanYil THEN Yil=Yas:CalisilanYil.

Display()
[END]
[END]
WHEN '?Button5'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4499
PROPERTY:END
If DEM:Hesaplama Then Cycle.
Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu
If Access:Tahakkuk.Fetch(Tah:keykod)<>Level:Benign Then Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Add(Tahakkuk).
DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil);

Nno#=(Yil-DEM:DonemYili)+1;
If DEG:KalanSifirEnfDevam And Yil>DEM:AmortismanEritmeYili+DEM:DonemYili
    If Message(Yil&' y˝l˝ tahakkuk bilgileri silinecek. Emin misiniz?','Amortisman Hesab˝',Icon:Question,BUTTON:YES+BUTTON:NO,BUTTON:NO)=BUTTON:YES
        Tah:YDOSabitKiymetDegeri[Nno#]=0; Tah:YDOAmortismanDegeri[Nno#]=0
        Tah:YDSSabitKiymetDegeri[Nno#]=0; Tah:YDSAmortismanDegeri[Nno#]=0
        Tah:AmortismanFarki[Nno#]=0; Tah:EskiNetDeger[Nno#]=0
        Tah:YeniNetDeger[Nno#]=0; Tah:DegerlemeFonu[Nno#]=0
        Tah:SabitKiymetNetArtisi[Nno#]=0;  Tah:BuYilAyrilanAmortisman[Nno#]=0
        Tah:BuYilBirikmisAmortisman[Nno#]=0; Tah:KalanNetDeger[Nno#]=0;
        Tah:DovizBuYilAyrilanAmortisman[Nno#]=0; Tah:DovizBuYilBirikmisAmortisman[Nno#]=0
        Tah:KalanDovizDeger[Nno#]=0 ; Tah:HesaplananAy[Nno#]=0
        Tah:BuYilGidereAtilmayanAmortisman[Nno#]=0
        Put(Tahakkuk)
    End
End

Loop i#=1 To 50; If Tah:BuYilAyrilanAmortisman[i#]+Tah:KalanNetDeger[i#]+Tah:YDSSabitKiymetDegeri[i#]=0 Then J#=i#-1; Break..

Yil#=(j#+DEM:DonemYili)-1
If Yil<Yil#
   Message('÷ncelikle '&Yil#&' y˝l˝n˝ silmelisiniz....','Amortisman Hesab˝',ICON:Exclamation); Cycle
End

If Yil=Yil# !Verilen Y˝l Son Y˝la = ise sil
    If (DEM:SermayeAktarimTarihi>=Yil#) Then
      Message(Yil#&' y˝l˝ itibariyle fon sermayeye aktar˝lm˝˛t˝r.|÷nce aktar˝m i˛lemini iptal ediniz..','Amortisman Hesab˝',ICON:Exclamation); Cycle.

    If j#=0 Then Message('B¸t¸n y˝llar silinmi˛tir.','Amortisman Hesab˝',ICON:Exclamation)
    Else
        DEM:KalanMiktar+=Adet;  Yil=(j#+DEM:DonemYili)-1
        If Message(Yil&' y˝l˝ tahakkuk bilgileri silinecek. Emin misiniz?','Amortisman Hesab˝',Icon:Question,BUTTON:YES+BUTTON:NO,BUTTON:NO)=BUTTON:YES
            Tah:YDOSabitKiymetDegeri[j#]=0; Tah:YDOAmortismanDegeri[j#]=0
            Tah:YDSSabitKiymetDegeri[j#]=0; Tah:YDSAmortismanDegeri[j#]=0
            Tah:AmortismanFarki[j#]=0; Tah:EskiNetDeger[j#]=0
            Tah:YeniNetDeger[j#]=0; Tah:DegerlemeFonu[j#]=0
            Tah:SabitKiymetNetArtisi[j#]=0;  Tah:BuYilAyrilanAmortisman[j#]=0
            Tah:BuYilBirikmisAmortisman[j#]=0; Tah:KalanNetDeger[j#]=0;
            Tah:DovizBuYilAyrilanAmortisman[j#]=0; Tah:DovizBuYilBirikmisAmortisman[j#]=0
            Tah:KalanDovizDeger[j#]=0 ; Tah:HesaplananAy[j#]=0; Tah:KistSonYilGidereAtilan[j#]=0
            Tah:BuYilGidereAtilmayanAmortisman[j#]=0
            Put(Tahakkuk)
            If ErrorCode() Then MEssage(ErrorCode()&' '&Error()).

            ! Tahakkuk Dosyas˝ndan Degerlemefonu lar˝n˝ topla Demirbas Yaz
            DEM:BirikmisDegerlemeFonu=0; Loop i#=1 To 50; DEM:BirikmisDegerlemeFonu+=Tah:DegerlemeFonu[i#].
            DEM:BirikmisDegerlemeFonu-=DEM:SermayeAktarilanFon
        End
        Put(Demirbas)
        Yil-=1; DISPLAY()
    End
End
Display
[END]
