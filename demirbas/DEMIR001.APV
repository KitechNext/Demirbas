EMBED %WindowManagerMethodCodeSection
[INSTANCES]
WHEN 'Init'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 9001
PROPERTY:END
Set(Yasal); Next(Yasal)
DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil)
eksikveri#=Level:Benign
IF DEG:HesapSekli='H' THEN
    LOOP yil#=2005 TO 2021
        DEG:DegerlemeYili=Yil#; IF Access:Degerleme.Fetch(DEG:KeyYil)<>Level:Benign THEN eksikveri#=Yil#; BREAK.
        LOOP ay#=1 TO 12
            IF yil#=Yil AND ay#>Ay THEN BREAK.
            IF DEG:KatsayiOrani[ay#]=0 THEN eksikveri#=Yil#; BREAK.
            IF yil#=2021 AND ay#=5 THEN Loc:DegerlemeBitis=DEG:KatsayiOrani[5]; BREAK.
        END
        IF eksikveri# THEN BREAK.
    END
    ! Kontrol sonras˝ olmas˝ gereken deerler geri dˆner..
    DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil)
END

IF eksikveri# THEN
    MESSAGE(eksikveri#&' y˝l˝na ait enflasyon verilerinde eksiklik var. L¸tfen kontrol ediniz.','Demirba˛ Hesab˝',ICON:Exclamation)
ELSE
    If Yil<YtlYil Then YuvOran=1 Else YuvOran=0.01.
    If Toplu<>'E' !Tekil Tahakkuk
        Clear(Dem:Record); DEM:DemirbasKodu=DKodBas; Get(Demirbas,DEM:keykod)
        If ErrorCode() Then Message(DKodBas&' bulunam˝yor.','Demirba˛ Hesab˝',ICON:Exclamation); Post(Event:CloseWindow). !Return 2.
        If ~DEM:Hesaplama
          Do SatisKontrolu; If ~DEM:KalanMiktar>0 Then Post(Event:CloseWindow). !Return 2.
          Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Get(Tahakkuk,Tah:keykod)
          If ErrorCode() Then Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Add(Tahakkuk).
          Line1=DEM:DemirbasKodu&' '&Clip(DEM:DemirbasAdi); Display
          No=(Yil-DEM:DonemYili)+1
          If (Yil>=DEM:DonemYili And No<=75) Then
            If ODnmHspl Then
              Clear(OTah:Record); OTah:DemirbasKodu=DEM:DemirbasKodu; Get(OTahakkuk,OTah:keykod); If ErrorCode() Then Clear(OTah:Record).
              If Yil=Dem:DonemYili Then
                !If DEG:OncekiDonemHesaplananYil=DEM:DonemYili And DEG:OncekiDonemHesaplananAy<Dem:DonemAy Then Do BirinciYilHesap Else Do OBirinciYilHesap.
                If Ay=Dem:DonemAy Then Do BirinciYilHesap Else Do OBirinciYilHesap.
              Else Do OHesapla.
            Else If Yil=Dem:DonemYili Then Do BirinciYilHesap Else Do Hesapla..
          End
          Do SatisKontroluIptal
        End
    Else   !Toplu Tahakkuk
        If ODnmHspl And DEG:OncekiDonemHesaplananYil='' And DEG:OncekiDonemHesaplananAy=0 Then Message('÷nceki Dˆnem Hesab˝ Bulunamam˝˛t˝r.','Demirba˛ Hesab˝',ICON:Exclamation); Post(Event:CloseWindow). !Return 2.
        DEG:HesaplananYil=Yil; DEG:HesaplananAy=Ay; Put(Degerleme)
        Clear(DEM:Record); DEM:DemirbasKodu=DKodBas; Set(DEM:keykod,DEM:keykod)
        Loop Until Access:Demirbas.Next()
          If ~(DEM:DemirbasKodu>=DKodBas And (DEM:DemirbasKodu<=DKodSon Or DKodSon='')) Then Break.
          If ~(DEM:AlimTarih>=BTrh And (DEM:AlimTarih<=STrh Or STrh='')) Then Cycle.
          If ~(DEM:DemirbasGrubu>=BGrp And (DEM:DemirbasGrubu<=SGrp Or SGrp='')) Then Cycle.
          Grb:GrupKodu=DEM:DemirbasGrubu; Access:Grub.Fetch(Grb:KeyKod)
          If ~(Grb:AmacKodu>=BAmc And (Grb:AmacKodu<=SAmc Or SAmc='')) Then Cycle.

          If ~DEM:Hesaplama
            Do SatisKontrolu; If ~DEM:KalanMiktar>0 Then Cycle.
            Line1=DEM:DemirbasKodu&' '&Clip(DEM:DemirbasAdi); Display
            No=(Yil-DEM:DonemYili)+1
            If Yil>=DEM:DonemYili And No<=75
              Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Get(Tahakkuk,Tah:keykod)
              If ErrorCode() Then Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Add(Tahakkuk).
              If ODnmHspl Then
                Clear(OTah:Record); OTah:DemirbasKodu=DEM:DemirbasKodu; Get(OTahakkuk,OTah:keykod); If ErrorCode() Then Clear(OTah:Record).
                If Yil=Dem:DonemYili Then
                  !If DEG:OncekiDonemHesaplananYil=DEM:DonemYili And DEG:OncekiDonemHesaplananAy<Dem:DonemAy Then Do BirinciYilHesap Else Do OBirinciYilHesap.
                  If Ay=Dem:DonemAy Then Do BirinciYilHesap Else Do OBirinciYilHesap.
                Else Do OHesapla.
              Else If Yil=Dem:DonemYili Then Do BirinciYilHesap Else Do Hesapla..
            End
            Do SatisKontroluIptal
          End
        End
    End
END

Post(Event:CloseWindow)
[END]
[END]
[END]
EMBED %ProcedureRoutines
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 3240
PROPERTY:END
SatisKontrolu Routine
      If Yil>=DEM:DonemYili   ! Bu Yil «˝k˝˛ Miktar˝
        SDE:DemirbasKodu=DEM:DemirbasKodu; SDE:SatisYil='0000'; Set(SDE:keykod,SDE:keykod); Adet=0
        Loop
          Next(SatDemir); If ErrorCode() Then Break.
          If ~(SDE:DemirbasKodu=DEM:DemirbasKodu) Then Break.
          If ~Ay=12 Then xTrh#=Date(Ay+1,1,Yil)-1; If SDE:SatisTarih=<XTrh# Then Adet+=SDE:SatisMiktar Else Break.
            Else If Yil>=SDE:SatisYil Then Adet+=SDE:SatisMiktar Else Break.
          End
        End
        DEM:KalanMiktar=DEM:Miktar; DEM:KalanMiktar-=Adet; Put(Demirbas);
      End


[SOURCE]
PROPERTY:BEGIN
PRIORITY 3240
PROPERTY:END
SatisKontroluIptal  Routine
      SDE:DemirbasKodu=DEM:DemirbasKodu; SDE:SatisYil='0000'; Set(SDE:keykod,SDE:keykod); Adet=0
      Loop
        Next(SatDemir); If ErrorCode() Then Break.
        If ~(SDE:DemirbasKodu=DEM:DemirbasKodu) Then Break.
        Adet+=SDE:SatisMiktar
      End
      DEM:KalanMiktar=DEM:Miktar; DEM:KalanMiktar-=Adet; Put(Demirbas);
[SOURCE]
PROPERTY:BEGIN
PRIORITY 3600
PROPERTY:END
BirinciYilHesap Routine
    DDebug(450,'Birinci y˝l hesab˝ Áal˝˛˝yor. Y˝l: '&Yil&' Demirba˛ Dˆnem Y˝l˝: '&DEM:DonemYili)
    Loc:HesapSekli=DEM:HesapSekli
    If Ay>=DEM:DonemAy
        If Yil>=2021
             DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil); DegerlemeOrani$=1; TefeYap$=False
             If DegYap='E' And ~DEM:Degerleme Then
                 CASE DEG:HesapSekli
                 OF 'H'
                     !IF DEM:AlimTarih<=DATE(06,09,2021) THEN  ! Yasa gerei 9 Haziran 2021 tarihi ˆncesinde al˝m˝ yap˝lm˝˛ demirba˛lar iÁin hesaplan˝r.
                     IF DEM:AlimTarih<DATE(05,01,2021) THEN  ! Yasa gerei 9 Haziran 2021 tarihi ˆncesinde al˝m˝ yap˝lm˝˛ demirba˛lar iÁin hesaplan˝r.
                         ! Demirba˛˝n ba˛lang˝Á oran˝n˝ al
                         IF DEM:DonemYili>=2005 THEN
                             IF DEM:DonemAy=12 THEN
                                 DEG:DegerlemeYili=DEM:DonemYili+1; Access:Degerleme.Fetch(DEG:KeyYil); Baslangic$=DEG:KatsayiOrani[1]
                             ELSE
                                 DEG:DegerlemeYili=DEM:DonemYili; Access:Degerleme.Fetch(DEG:KeyYil); Baslangic$=DEG:KatsayiOrani[DEM:DonemAy+1]
                             END
                         ELSE
                             DEG:DegerlemeYili='2005'; Access:Degerleme.Fetch(DEG:KeyYil); Baslangic$=DEG:KatsayiOrani[1]
                         END
                         DDebug(455,'Deerleme Y˝l˝: '&DEG:DegerlemeYili&' Ba˛lang˝Á: '&Baslangic$&' Biti˛: '&Loc:DegerlemeBitis)
                         DegerlemeOrani$=(Loc:DegerlemeBitis/Baslangic$)
                         DEM:HareketliDegerlemeOrani=DegerlemeOrani$
                         DEM:HareketliDegerlemeTarihi=DATE(Ay,1,Yil)

                         ! Hesaplama sonras˝ deerleme bilgilerini geri getir
                         DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil)
                     ELSE
                         DegerlemeOrani$=1
                     END
                     TefeYap$=True
                 OF 'E'
                        DegerlemeOrani$=1+DEG:TefeOrani[Ay]; TefeYap$=True
                 OF 'D'
                    !!! Ceyo ˆzel Sﬁ 21.02.2024
                    IF Glo:Ceyo=TRUE
                        Loc:EnflasyonDegeri=1
                        IF Yil=2023
                            IF YEAR(DEM:AlimTarih)=2023 THEN
                                DEM:HesapSekli='H'; TefeYap$=TRUE
                                DegerlemeOrani$=1
                                Loc:EnflasyonDegeri=(DEG:KatsayiOrani[12]/DEG:KatsayiOrani[MONTH(DEM:AlimTarih)])
                                !DEM:HareketliDegerlemeOrani=DegerlemeOrani$; DEM:HareketliDegerlemeTarihi=DATE(Ay,1,Yil)
                            END
                        END
                    END
                 END
                 DDebug(456,'Deerleme Hesap ﬁekli: '&DEG:HesapSekli&' Oran˝: '&DegerlemeOrani$)

                 If Yil=DEM:DonemYili And Ay=DEM:DonemAy Then DegerlemeOrani$=1.
             Else Clear(Deg:Record).
        ElsIf Yil>=2005
             DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil); DegerlemeOrani$=1; TefeYap$=False
             If DegYap='E' And ~DEM:Degerleme Then
               If DEG:HesapSekli='E' Then DegerlemeOrani$=1+DEG:TefeOrani[Ay]; TefeYap$=True.
               If Yil=DEM:DonemYili And Ay=DEM:DonemAy Then DegerlemeOrani$=1.
             Else Clear(Deg:Record).
        Else
             DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil); DegerlemeOrani$=1; TefeYap$=False
             If DegYap='E' And ~DEM:Degerleme
             !÷nceki Y˝l Tefe Bilgisi Al
               Clear(Deg:Record); DEG:DegerlemeYili=DEM:DonemYili; Access:Degerleme.Fetch(DEG:KeyYil)
               If ErrorCode() Then Clear(Deg:Record); Message(Yil&' Y˝l˝ Deerleme Bilgileri Yok!','Demirba˛ Hesab˝',ICON:Exclamation).
               Tefe2$=DEG:TefeOrani[DEM:DonemAy];

               !Bu Y˝l Tefe & Degerleme Oran Bilgisi Al
               Clear(Deg:Record); DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil)
               If ErrorCode() Then Clear(Deg:Record); Message(Yil&' Y˝l˝ Deerleme Bilgileri Yok!','Demirba˛ Hesab˝',ICON:Exclamation).
               Tefe1$=DEG:TefeOrani[Ay];

               If DEG:HesapSekli='E' And Yil=>2004 Then DegerlemeOrani$=(Tefe1$/Tefe2$); DegerlemeOrani$=Round(DegerlemeOrani$,.00001); TefeYap$=True
               Else DegerlemeOrani$=1.
             Else Clear(Deg:Record).
        End
        AmortismanAyirmaOrani$=DEM:AmortismanAyirmaOrani[1]
        AmortismanEritmeYili$=DEM:AmortismanEritmeYili

        If DEM:HesapSekli='H' !%50 GeÁmemeli
           If AmortismanAyirmaOrani$>50 Then AmortismanAyirmaOrani$=50.
        End

        DemTutar$=0; If Toyotetsu And DEM:AlimTarih<Date(1,1,2005) Then DemTutar$=Dem:Tutar; DEM:Tutar=Round(DEM:Tutar/1000000,YuvOran).

        DDebug(455,'Amortisman Ay˝rma ﬁekli: '&DEM:AmortismanAyirmaSekli&' Oran˝: '&DegerlemeOrani$)
        Case DEM:AmortismanAyirmaSekli
        Of 'D'; OrOf 'A'; OrOf 'B'
            If TefeYap$
              Tah:YDOSabitKiymetDegeri[1]=DEM:Tutar; Tah:YDOAmortismanDegeri[1]=0
              Tah:YDSSabitKiymetDegeri[1]=Tah:YDOSabitKiymetDegeri[1]*DegerlemeOrani$
              IF Glo:Ceyo THEN Tah:YDSSabitKiymetDegeri[1]*=Loc:EnflasyonDegeri.  !!! Ceyo ˆzel Sﬁ 21.02.2024

              Tah:EskiNetDeger[1]=Tah:YDOSabitKiymetDegeri[1]
              Tah:YeniNetDeger[1]=Tah:YDSSabitKiymetDegeri[1]
              IF (DEG:HesapSekli='D' OR DEG:HesapSekli='H') Then Tah:DegerlemeFonu[No]=Tah:YeniNetDeger[1]-Tah:EskiNetDeger[1] ELSE Tah:DegerlemeFonu[No]=0.
              Tah:SabitKiymetNetArtisi[1]=Tah:YDSSabitKiymetDegeri[1]-Tah:YDOSabitKiymetDegeri[1]
              ! Esat
              If ~DEM:AmortismanAyirma
                    Case DEM:HesapSekli
                    Of 'N'; Tah:BuYilAyrilanAmortisman[1]=Round(((Tah:YDSSabitKiymetDegeri[1]*(AmortismanAyirmaOrani$/100))/12)*Ay,YuvOran)
                    Of 'H'; Tah:BuYilAyrilanAmortisman[1]=Round(((Tah:YeniNetDeger[1]*(AmortismanAyirmaOrani$/100))/12)*Ay,YuvOran)
                    End
              Else Tah:BuYilAyrilanAmortisman[1]=0.
            Else
              If ~DEM:AmortismanAyirma Then Tah:BuYilAyrilanAmortisman[1]=Round(((DEM:Tutar*(AmortismanAyirmaOrani$/100))/12)*Ay,YuvOran)
              Else Tah:BuYilAyrilanAmortisman[1]=0.
            End
        Of 'T' !Kist Ay Bulunuyor
            If TefeYap$=True
              Tah:YDOSabitKiymetDegeri[1]=DEM:Tutar; Tah:YDOAmortismanDegeri[1]=0
              Tah:YDSSabitKiymetDegeri[1]=Tah:YDOSabitKiymetDegeri[1]*DegerlemeOrani$
              IF Glo:Ceyo THEN Tah:YDSSabitKiymetDegeri[1]*=Loc:EnflasyonDegeri.  !!! Ceyo ˆzel Sﬁ 21.02.2024

              Tah:EskiNetDeger[1]=Tah:YDOSabitKiymetDegeri[1]
              Tah:YeniNetDeger[1]=Tah:YDSSabitKiymetDegeri[1]
              Tah:DegerlemeFonu[No]=0
              Tah:SabitKiymetNetArtisi[1]=Tah:YDSSabitKiymetDegeri[1]-Tah:YDOSabitKiymetDegeri[1]

              If Month(DEM:AlimTarih) <= Yas:DonemBasiAy Then KistAy=(Yas:DonemSonuAy-Month(DEM:AlimTarih))+1 Else KistAy=12-(Month(DEM:AlimTarih)-Yas:DonemBasiAy).
              If ~DEM:AmortismanAyirma
                Case DEM:KistHesapSekli
                Of 'Y'; Tah:BuYilAyrilanAmortisman[1]=Round(((((Tah:YDSSabitKiymetDegeri[1]/12)*KistAy)*(AmortismanAyirmaOrani$/100))/12)*((Ay-DEM:DonemAy)+1),YuvOran)
                Of 'A'; OrOf 'O'; OrOf 'P'; Tah:BuYilAyrilanAmortisman[1]=Round(((Tah:YDSSabitKiymetDegeri[1]*(AmortismanAyirmaOrani$/100))/12)*((Ay-DEM:DonemAy)+1),YuvOran)
                Of 'U'; Tah:BuYilAyrilanAmortisman[1]=Round(((Tah:YDSSabitKiymetDegeri[1]*(AmortismanAyirmaOrani$/100))/12),YuvOran)
                End
              Else Tah:BuYilAyrilanAmortisman[1]=0.
            Else
              If Month(DEM:AlimTarih)<=Yas:DonemBasiAy Then KistAy=(Yas:DonemSonuAy-Month(DEM:AlimTarih))+1 Else KistAy=12-(Month(DEM:AlimTarih)-Yas:DonemBasiAy).
              If ~DEM:AmortismanAyirma
                Case DEM:KistHesapSekli
                Of 'Y'; Tah:BuYilAyrilanAmortisman[1]=Round(((((DEM:Tutar/12)*KistAy)*(AmortismanAyirmaOrani$/100))/12)*((Ay-DEM:DonemAy)+1),YuvOran)
                Of 'A'; OrOf 'O'; OrOf 'P'; Tah:BuYilAyrilanAmortisman[1]=Round(((Dem:Tutar*(AmortismanAyirmaOrani$/100))/12)*((Ay-DEM:DonemAy)+1),YuvOran)
                End
              Else Tah:BuYilAyrilanAmortisman[1]=0.
            End
            If DEM:KistHesapSekli='U' Then Tah:KistSonYilGidereAtilan[1]=0
            Else Tah:KistSonYilGidereAtilan[1]=(Dem:Tutar*(AmortismanAyirmaOrani$/100))-Tah:BuYilAyrilanAmortisman[1].
        End ! Case

        Tah:Miktar[1]=DEM:KalanMiktar
        If DEM:AmortismanAyirmaSekli='T' And DEM:KistHesapSekli='P'
            Tah:BuYilBirikmisAmortisman[1]=Tah:BuYilAyrilanAmortisman[1]+Tah:KistSonYilGidereAtilan[1]
        Else Tah:BuYilBirikmisAmortisman[1]=Tah:BuYilAyrilanAmortisman[1].

        If Tefeyap$ !Yil>=2004 And Year(DEM:AlimTarih)=>2004
            If DEM:AmortismanAyirmaSekli='T' And (DEM:KistHesapSekli='O' Or DEM:KistHesapSekli='P')
              If DEM:KistHesapSekli='O' Then Tah:KalanNetDeger[1]=Tah:YeniNetDeger[1]-(Tah:BuYilBirikmisAmortisman[1]+Tah:KistSonYilGidereAtilan[1]).
              If DEM:KistHesapSekli='P' Then Tah:KalanNetDeger[1]=Tah:YeniNetDeger[1]-(Tah:BuYilBirikmisAmortisman[1]).
            Else Tah:KalanNetDeger[1]=Tah:YeniNetDeger[1]-Tah:BuYilBirikmisAmortisman[1].
        Else
            If DEM:AmortismanAyirmaSekli='T' And (DEM:KistHesapSekli='O' Or DEM:KistHesapSekli='P')
              If DEM:KistHesapSekli='O' Then Tah:KalanNetDeger[1]=DEM:Tutar-(Tah:BuYilBirikmisAmortisman[1]+Tah:KistSonYilGidereAtilan[1]).
              If DEM:KistHesapSekli='P' Then Tah:KalanNetDeger[1]=DEM:Tutar-(Tah:BuYilBirikmisAmortisman[1]).
            Else Tah:KalanNetDeger[1]=DEM:Tutar-Tah:BuYilBirikmisAmortisman[1].
        End

        If DEM:Miktar<>DEM:KalanMiktar
            Tah:BuYilBirikmisAmortisman[1]=Round((Tah:BuYilBirikmisAmortisman[1]/DEM:Miktar)*DEM:KalanMiktar,YuvOran)
            If DEM:AmortismanAyirmaSekli='T' And (DEM:KistHesapSekli='O' Or DEM:KistHesapSekli='P')
              If DEM:KistHesapSekli='O' Then Tah:KalanNetDeger[1]=Round(((Tah:KalanNetDeger[1]-Tah:KistSonYilGidereAtilan[1])/DEM:Miktar)*DEM:KalanMiktar,YuvOran).
              If DEM:KistHesapSekli='P' Then Tah:KalanNetDeger[1]=Round(((Tah:KalanNetDeger[1])/DEM:Miktar)*DEM:KalanMiktar,YuvOran).
            Else
              Tah:KalanNetDeger[1]=Round((Tah:KalanNetDeger[1]/DEM:Miktar)*DEM:KalanMiktar,YuvOran)
            End
            If TefeYap$
              Tah:YDOSabitKiymetDegeri[1]=Round((Tah:YDOSabitKiymetDegeri[1]/DEM:Miktar)*DEM:KalanMiktar,YuvOran)
              Tah:YDSSabitKiymetDegeri[1]=Round((Tah:YDSSabitKiymetDegeri[1]/DEM:Miktar)*DEM:KalanMiktar,YuvOran)
              Tah:EskiNetDeger[1]=Tah:YDOSabitKiymetDegeri[1]
              Tah:YeniNetDeger[1]=Tah:YDSSabitKiymetDegeri[1]
            End
        End

        Tah:YDOSabitKiymetDegeri[No]=Round(Tah:YDOSabitKiymetDegeri[1],YuvOran)
        Tah:YDOAmortismanDegeri[No]=Round(Tah:YDOAmortismanDegeri[1],YuvOran)
        Tah:YDSSabitKiymetDegeri[No]=Round(Tah:YDSSabitKiymetDegeri[1],YuvOran)
        Tah:YDSAmortismanDegeri[No]=Round(Tah:YDSAmortismanDegeri[1],YuvOran)
        Tah:AmortismanFarki[No]=Round(Tah:AmortismanFarki[1],YuvOran)
        Tah:EskiNetDeger[No]=Round(Tah:EskiNetDeger[1],YuvOran)
        Tah:YeniNetDeger[NO]=Round(Tah:YeniNetDeger[1],YuvOran)
        Tah:DegerlemeFonu[No]=Round(Tah:DegerlemeFonu[1],YuvOran)
        Tah:SabitKiymetNetArtisi[No]=Round(Tah:SabitKiymetNetArtisi[1],YuvOran)
        Tah:BuYilAyrilanAmortisman[No]=Round(Tah:BuYilAyrilanAmortisman[1],YuvOran)
        Tah:BuYilBirikmisAmortisman[No]=Round(Tah:BuYilBirikmisAmortisman[1],YuvOran)
        Tah:KalanNetDeger[No]=Round(Tah:KalanNetDeger[1],YuvOran)

        Tah:HesaplananAy[1]=Ay
        If GidereAtilmayan Then Tah:BuYilGidereAtilmayanAmortisman[1]=Tah:BuYilAyrilanAmortisman[1] Else Tah:BuYilGidereAtilmayanAmortisman[1]=0.

        Put(Tahakkuk)
        If ~DemTutar$=0 Then Dem:Tutar=DemTutar$.
        ! Tahakkuk Dosyas˝ndan Degerlemefonu lar˝n˝ topla Demirbas Yaz
        DEM:BirikmisDegerlemeFonu=0; Loop i#=1 To 50; DEM:BirikmisDegerlemeFonu+=Tah:DegerlemeFonu[i#].;
        DEM:BirikmisDegerlemeFonu-=DEM:SermayeAktarilanFon
        DEM:HesapSekli=Loc:HesapSekli
        Put(Demirbas)
        DDebug(459,'Demirba˛ deerleri g¸ncellendi. Tarihi: '&FORMAT(DEM:HareketliDegerlemeTarihi,@d17)&' Oran˝: '&DEM:HareketliDegerlemeOrani)
    End
[SOURCE]
PROPERTY:BEGIN
PRIORITY 3600
PROPERTY:END
Hesapla             ROUTINE
    DDebug(450,'Hesaplama ('&CLIP(DEM:DemirbasKodu)&') iÁin Áal˝˛t˝. Y˝l: '&Yil&' Demirba˛ Dˆnem Y˝l˝: '&DEM:DonemYili)
    Loc:HesapSekli=DEM:HesapSekli
    Loop
        If Yil>DEM:DonemYili Then
            No=(Yil-DEM:DonemYili)+1
            IF Yil>=2005
                DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil); DegerlemeOrani$=1; TefeYap$=False
                If DegYap='E' And ~DEM:Degerleme Then
                    CASE DEG:HesapSekli
                    OF 'E'
                        DegerlemeOrani$=1+DEG:TefeOrani[Ay]
                        TefeYap$=True
                    OF 'H'
                        !!!IF Yil>2021 AND Yil=DEM:DonemYili THEN Tah:BuYilBirikmisAmortisman[No+1]=.
                        !IF DEM:AlimTarih<=DATE(06,09,2021) THEN  ! Yasa gerei 9 Haziran 2021 tarihi ˆncesinde al˝m˝ yap˝lm˝˛ demirba˛lar iÁin hesaplan˝r.
                        IF DEM:AlimTarih<DATE(05,01,2021) THEN  ! Yasa gerei 9 Haziran 2021 tarihi ˆncesinde al˝m˝ yap˝lm˝˛ demirba˛lar iÁin hesaplan˝r. Son ay hesaplanmad˝˝ iÁin 1 May˝s ˆncesi hesaba kat˝l˝yor.
                            ! Demirba˛˝n ba˛lang˝Á oran˝n˝ al
                            IF DEM:DonemYili>=2005 THEN
                                IF DEM:DonemAy=12 THEN
                                    DEG:DegerlemeYili=DEM:DonemYili+1; Access:Degerleme.Fetch(DEG:KeyYil); Baslangic$=DEG:KatsayiOrani[1]
                                ELSE
                                    DEG:DegerlemeYili=DEM:DonemYili; Access:Degerleme.Fetch(DEG:KeyYil); Baslangic$=DEG:KatsayiOrani[DEM:DonemAy+1]
                                END
                            ELSE
                                DEG:DegerlemeYili='2005'; Access:Degerleme.Fetch(DEG:KeyYil); Baslangic$=DEG:KatsayiOrani[1]
                            END
                            DegerlemeOrani$=ROUND(Loc:DegerlemeBitis/Baslangic$,.00001)
                            DEM:HareketliDegerlemeOrani=DegerlemeOrani$
                            DEM:HareketliDegerlemeTarihi=DATE(Ay,1,Yil)
                            DDebug(452,'Deerleme Y˝l˝: '&DEG:DegerlemeYili&' Ba˛lang˝Á: '&Baslangic$&' Biti˛: '&Loc:DegerlemeBitis)

                            ! Hesaplama sonras˝ deerleme bilgilerini geri getir
                            DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil)
                        ELSE
                            DDebug(456,'Demirba˛ al˝m tarihi '&FORMAT(DEM:AlimTarih,@d06.)&' olduu iÁin yasa gerei deerleme oran˝ 1 yap˝ld˝.')
                            DegerlemeOrani$=1
                        END
                        TefeYap$=True
                    OF 'D' ! Sﬁ 20.10.2021
                        DegerlemeOrani$=1+(DEG:DegerlemeOrani/100)
                        !!! Ceyo ˆzel Sﬁ 21.02.2024
                        IF Glo:Ceyo=TRUE
                            IF Yil=2023
                                IF YEAR(DEM:AlimTarih)<2023 THEN
                                    !DegerlemeOrani$=1+(((DEG:DegerlemeOrani*9/12/100)+((DEG:KatsayiOrani[12]/DEG:KatsayiOrani[9])-1)))
                                    Loc:EnflasyonDegeri=((DEG:KatsayiOrani[12]/DEG:KatsayiOrani[9]))
                                    DDebug(453,'Deerleme Oran˝: '&DegerlemeOrani$&' Enflasyon Oran˝: '&Loc:EnflasyonDegeri)
                                ELSE
                                    DEG:HesapSekli='H'
                                    DEM:HareketliDegerlemeOrani=DegerlemeOrani$; DEM:HareketliDegerlemeTarihi=DATE(Ay,1,Yil)
                                    DegerlemeOrani$=(DEG:KatsayiOrani[12]/DEG:KatsayiOrani[MONTH(DEM:AlimTarih)])
                                END
                            END
                        END
                    END
                Else Clear(Deg:Record).
            Else
                DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil); DegerlemeOrani$=1
                If DegYap='E' And ~DEM:Degerleme
                    !÷nceki Y˝l Tefe Bilgisi Al
                    Clear(Deg:Record); DEG:DegerlemeYili=Yil-1; Access:Degerleme.Fetch(DEG:KeyYil)
                    If ErrorCode() Then Clear(Deg:Record); Message(Yil&' Y˝l˝ Deerleme Bilgileri Yok!','Demirba˛ Hesab˝',ICON:Exclamation).
                    Tefe2$=DEG:TefeOrani[12];

                    !Bu Y˝l Tefe & Deerleme Oran Bilgisi Al
                    Clear(Deg:Record); DEG:DegerlemeYili=Yil; Access:Degerleme.Fetch(DEG:KeyYil)
                    If ErrorCode() Then Clear(Deg:Record); Message(Yil&' Y˝l˝ Deerleme Bilgileri Yok!','Demirba˛ Hesab˝',ICON:Exclamation).
                    Tefe1$=DEG:TefeOrani[Ay];

                    If DEG:HesapSekli='E' And Yil=>2004 Then
                        DegerlemeOrani$=(Tefe1$/Tefe2$); DegerlemeOrani$=Round(DegerlemeOrani$,.00001)
                    Else DegerlemeOrani$=1+(DEG:DegerlemeOrani/100).
                Else Clear(Deg:Record).
            End

            ! Amortisman s¸resi dolduktan sonra deerleme yap˝lmas˝ istenmiyorsa
            ! Sﬁ 24.01.2022

            IF Par:BitmisleriDegerle=False AND DEM:DonemYili+DEM:AmortismanEritmeYili<=Yil THEN
                DegerlemeOrani$=1
                DDebug(454,'Amortisman deerlemesi bittii iÁin oran 1 yap˝ld˝')
            ELSE
                IF Par:BitmisleriDegerle=1 AND DEM:AmortismanAyirmaSekli='B' AND DEM:DonemYili+DEM:AmortismanEritmeYili<=Yil THEN
                    DegerlemeOrani$=1
                    DDebug(456,'Bina deerlemesi bittii iÁin oran 1 yap˝ld˝')
                    !Tah:YDOSabitKiymetDegeri[No]=Round(Tah:YDSSabitKiymetDegeri[No-1]*DegerlemeOrani$,.01)
                    !Tah:YDOAmortismanDegeri[No]=Round(Tah:BuYilBirikmisAmortisman[No-1]*DegerlemeOrani$,.01)
                END
            END
            DDebug(460,'Deerleme Hesap ﬁekli: '&DEG:HesapSekli&' Oran˝: '&DegerlemeOrani$)

            !!! Deerleme i˛lemi
            !If (Tah:BuYilAyrilanAmortisman[No-1]<=0 And (DEM:AmortismanAyirma And Tah:KalanNetDeger[No-1]<=0)) And ~(DEG:KalanSifirEnfDevam And (DEG:HesapSekli='E' OR DEG:HesapSekli='H') And Yil=>2004)
            If (Tah:BuYilAyrilanAmortisman[No-1]<=0 And (DEM:AmortismanAyirma And Tah:KalanNetDeger[No-1]<=0)) And ~(DEG:KalanSifirEnfDevam And Yil=>2004)
                If Toplu<>'E' Then Message(DEM:DemirbasKodu&' ˆnceki dˆnem hesaplanmal˝d˝r...!','Demirba˛ Hesab˝',ICON:Exclamation).
            Else
                If Tah:KalanNetDeger[No-1]=0 Then   ! Son y˝ldan sonra
                  !If DEG:KalanSifirEnfDevam And (DEG:HesapSekli='E' OR DEG:HesapSekli='H') And Yil=>2004 Then
                  If DEG:KalanSifirEnfDevam And Yil=>2004 Then
                    If Yil=2004 !!! 2021 yeni yap˝lan
                        Tah:YDOSabitKiymetDegeri[No]=Tah:DuzeltmeyeEsasTutar
                        Tah:YDOAmortismanDegeri[No]=Tah:DuzeltilmisBirikmisAmortisman
                    ElsIf Yil=2005      ! Paradan milyon at˝ld˝.
                        Tah:YDOSabitKiymetDegeri[No]=Round(Tah:YDSSabitKiymetDegeri[No-1]/1000000,.01)
                        Tah:YDOAmortismanDegeri[No]=Round(Tah:BuYilBirikmisAmortisman[No-1]/1000000,.01)
                    ElsIf Yil>=2021
                        ! Yeni mevzuat gerei ˆnceki deerleme tutar˝ bulunuyor.
                        IF Tah:BuYilBirikmisAmortisman[No-1]=0 THEN
                            DDebug(450,'÷nceki y˝la ait birikmi˛ amortisman yok. ÷nceki y˝llar taran˝yor.')
                            LOOP j#=No-2 TO 1 BY -1
                                IF Tah:BuYilBirikmisAmortisman[j#]>0 THEN
                                    DDebug(450,'Son birikmi˛ amortisman˝n y˝l˝: '&DEM:DonemYili+j#-1)

                                    Tah:YDOSabitKiymetDegeri[No]=Round(Tah:YDSSabitKiymetDegeri[j#],.01)
                                    Tah:YDOAmortismanDegeri[No]=Round(Tah:BuYilBirikmisAmortisman[j#],.01)
                                    ! Kalan net deer s˝f˝rsa ve 2004 ˆncesi ise Tah:DuzeltmeyeEsasTutar deeri kullan˝ls˝n.
                                    IF DEM:DonemYili+j#-1<2004 AND Tah:KalanNetDeger[j#]=0 THEN Tah:YDOSabitKiymetDegeri[No]=Round(Tah:DuzeltmeyeEsasTutar,.01); Tah:YDOAmortismanDegeri[No]=Round(Tah:DuzeltmeyeEsasTutar,.01).   !Tah:DuzeltilmisBirikmisAmortisman

                                    IF DEM:DonemYili+j#-1<2005      ! Deer 2005 ˆncesine aitse liradan milyon at˝l˝r.
                                        Tah:YDOSabitKiymetDegeri[No]=Round(Tah:YDOSabitKiymetDegeri[No]/1000000,.01)
                                        Tah:YDOAmortismanDegeri[No]=Round(Tah:YDOAmortismanDegeri[No]/1000000,.01)
                                    END
                                    BREAK
                                END
                            END
                        ELSE
                            DDebug(450,'÷nceki y˝la ait birikmi˛ amortisman '&Tah:BuYilBirikmisAmortisman[No-1])
                            Tah:YDOSabitKiymetDegeri[No]=Round(Tah:YDSSabitKiymetDegeri[No-1],.01)
                            Tah:YDOAmortismanDegeri[No]=Round(Tah:BuYilBirikmisAmortisman[No-1],.01)
                        END
                    Else
                        Tah:YDOSabitKiymetDegeri[No]=Round(Tah:YDSSabitKiymetDegeri[No-1],.01)
                        Tah:YDOAmortismanDegeri[No]=Round(Tah:BuYilBirikmisAmortisman[No-1],.01)
                    End
                    Tah:YDSSabitKiymetDegeri[No]=Round((Tah:YDOSabitKiymetDegeri[No]*DegerlemeOrani$),.01)
                    IF Glo:Ceyo THEN Tah:YDSSabitKiymetDegeri[No]=Round((Tah:YDOSabitKiymetDegeri[No]*DegerlemeOrani$*Loc:EnflasyonDegeri),.01).  !!! Ceyo ˆzel Sﬁ 21.02.2024
                    Tah:YDSAmortismanDegeri[No]=Round((Tah:YDOAmortismanDegeri[No]*DegerlemeOrani$),.01)
                    IF Glo:Ceyo THEN Tah:YDSAmortismanDegeri[No]=Round((Tah:YDOAmortismanDegeri[No]*DegerlemeOrani$*Loc:EnflasyonDegeri),.01).  !!! Ceyo ˆzel Sﬁ 21.02.2024
                    Tah:AmortismanFarki[No]=Round((Tah:YDSAmortismanDegeri[No]-Tah:YDOAmortismanDegeri[No]),.01)
                    Tah:BuYilBirikmisAmortisman[No]=Round(Tah:YDSAmortismanDegeri[No],.01)
                    Tah:SabitKiymetNetArtisi[No]=Round((Tah:YDSSabitKiymetDegeri[No]-Tah:YDOSabitKiymetDegeri[No]),.01)
                    Tah:Miktar[No]=DEM:KalanMiktar
                    !Message(DEM:Miktar&' '&DEM:KalanMiktar&' '&Tah:Miktar[No-1])
                    If DEM:Miktar<>DEM:KalanMiktar
                        Tah:YDOSabitKiymetDegeri[No]=Round((Tah:YDOSabitKiymetDegeri[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:YDOAmortismanDegeri[No]=Round((Tah:YDOAmortismanDegeri[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:YDSSabitKiymetDegeri[No]=Round((Tah:YDSSabitKiymetDegeri[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:YDSAmortismanDegeri[No]=Round((Tah:YDSAmortismanDegeri[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:AmortismanFarki[No]=Round((Tah:AmortismanFarki[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:EskiNetDeger[No]=Round((Tah:EskiNetDeger[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:YeniNetDeger[NO]=Round((Tah:YeniNetDeger[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:DegerlemeFonu[No]=Round((Tah:DegerlemeFonu[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:SabitKiymetNetArtisi[No]=Round((Tah:SabitKiymetNetArtisi[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:BuYilAyrilanAmortisman[No]=Round((Tah:BuYilAyrilanAmortisman[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:BuYilBirikmisAmortisman[No]=Round((Tah:BuYilBirikmisAmortisman[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:KalanNetDeger[No]=Round((Tah:KalanNetDeger[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                    End

                    Put(Tahakkuk)
                  Else
                    If Toplu<>'E' Then Message(DEM:DemirbasKodu&' Demirba˛ S˝f˝rlanm˝˛t˝r.!','Demirba˛ Hesab˝',ICON:Exclamation).
                  End
                Else
                    AmortismanAyirmaOrani$=DEM:AmortismanAyirmaOrani[No]
                    IF DEM:KistHesapSekli='U' Then
                      If No>DEM:AmortismanEritmeYili Then AmortismanAyirmaOrani$=DEM:AmortismanAyirmaOrani[No-1].
                      If Dem:DonemAy>1 Then AmortismanEritmeYili$=DEM:AmortismanEritmeYili+1 Else AmortismanEritmeYili$=DEM:AmortismanEritmeYili.
                    Else AmortismanEritmeYili$=DEM:AmortismanEritmeYili.

                    If DEM:AmortismanAyirmaSekli='T' And DEM:KistHesapSekli='U'
                    If No>AmortismanEritmeYili$ Then Break.
                    If No=AmortismanEritmeYili$ And Dem:DonemAy=1 And Ay>12 Then Break.
                    If No=AmortismanEritmeYili$ And Dem:DonemAy>1 And Ay>Dem:DonemAy-1 Then Break.
                    End

                    If DEM:HesapSekli='H' !%50 GeÁmemeli
                      If AmortismanAyirmaOrani$>50 Then AmortismanAyirmaOrani$=50.
                    End

                    Case DEM:AmortismanAyirmaSekli
                    Of 'D' ! Demirbas
                        DO NormalDemirbasHesabi
                    Of 'B' ! Bina
                        IF Yil<2021
                            DO BinaDemirbasHesabi
                        ELSE
                            DO NormalDemirbasHesabi
                        END

                    Of 'A' ! Arsa
                        DO ArsaDemirbasHesabi
                    Of 'T' !Tasit
                        DO TasitDemirbasHesabi
                    End

                    Tah:Miktar[No]=DEM:KalanMiktar
                    !Satis Kist Uygula
                    If DEM:Miktar<>DEM:KalanMiktar
                        Tah:YDOSabitKiymetDegeri[No]=Round((Tah:YDOSabitKiymetDegeri[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:YDOAmortismanDegeri[No]=Round((Tah:YDOAmortismanDegeri[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:YDSSabitKiymetDegeri[No]=Round((Tah:YDSSabitKiymetDegeri[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:YDSAmortismanDegeri[No]=Round((Tah:YDSAmortismanDegeri[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:AmortismanFarki[No]=Round((Tah:AmortismanFarki[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:EskiNetDeger[No]=Round((Tah:EskiNetDeger[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:YeniNetDeger[NO]=Round((Tah:YeniNetDeger[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:DegerlemeFonu[No]=Round((Tah:DegerlemeFonu[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:SabitKiymetNetArtisi[No]=Round((Tah:SabitKiymetNetArtisi[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:BuYilAyrilanAmortisman[No]=Round((Tah:BuYilAyrilanAmortisman[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:BuYilBirikmisAmortisman[No]=Round((Tah:BuYilBirikmisAmortisman[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                        Tah:KalanNetDeger[No]=Round((Tah:KalanNetDeger[no]/Tah:Miktar[No-1])*DEM:KalanMiktar,YuvOran)
                    Else
                        Tah:YDOSabitKiymetDegeri[No]=Round(Tah:YDOSabitKiymetDegeri[no],YuvOran)
                        Tah:YDOAmortismanDegeri[No]=Round(Tah:YDOAmortismanDegeri[no],YuvOran)
                        Tah:YDSSabitKiymetDegeri[No]=Round(Tah:YDSSabitKiymetDegeri[no],YuvOran)
                        Tah:YDSAmortismanDegeri[No]=Round(Tah:YDSAmortismanDegeri[no],YuvOran)
                        Tah:AmortismanFarki[No]=Round(Tah:AmortismanFarki[no],YuvOran)
                        Tah:EskiNetDeger[No]=Round(Tah:EskiNetDeger[no],YuvOran)
                        Tah:YeniNetDeger[NO]=Round(Tah:YeniNetDeger[no],YuvOran)
                        Tah:DegerlemeFonu[No]=Round(Tah:DegerlemeFonu[no],YuvOran)
                        Tah:SabitKiymetNetArtisi[No]=Round(Tah:SabitKiymetNetArtisi[no],YuvOran)
