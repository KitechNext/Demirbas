WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
Clear(Tah:Record); Tah:DemirbasKodu=Dem:DemirbasKodu; Access:Tahakkuk.Fetch(Tah:keykod)

SDE:Tutar=SDE:SatisMiktar*SDE:BirimTutar;Display
If SDE:SatisMiktar<>0
  Loop i#=75 To 1 by -1; If ~(Tah:BuYilBirikmisAmortisman[i#]+Tah:KalanNetDeger[i#])=0 Then J#=i#; Break..
  SDE:SonNetDeger=Round((Tah:KalanNetDeger[j#]/DEM:KalanMiktar)*SDE:SatisMiktar,.01) ;
  SDE:SonBrutDeger=Round((Tah:YDSSabitKiymetDegeri[j#]/DEM:KalanMiktar)*SDE:SatisMiktar,.01)

  If Tah:DuzeltmeyeEsasTutar=0
    SDE:IlkAlimBedeli=Round(DEM:BirimTutar*SDE:SatisMiktar,.01)
  Else
    SDE:IlkAlimBedeli=Round((Tah:DuzeltmeyeEsasTutar/DEM:KalanMiktar)*SDE:SatisMiktar,.01)
  End



  SDE:BirikmisAmortismanFarki=0;
  Loop i#=1 To j#; SDE:BirikmisAmortismanFarki+=Round((Tah:AmortismanFarki[i#]/Tah:Miktar[i#])*SDE:SatisMiktar,.01).
  SDE:BirikmisDegerlemeFonu=0;
!  Loop i#=1 To No-1;SDE:BirikmisDegerlemeFonu+=Round((Tah:DegerlemeFonu[i#]/Tah:Miktar[i#])*SDE:SatisMiktar,.01).
  Loop i#=1 To j#; SDE:BirikmisDegerlemeFonu+=Round((Tah:DegerlemeFonu[i#]/Tah:Miktar[i#])*SDE:SatisMiktar,.01).
  SDE:BirikmisDegerlemeFonu-=DEM:SermayeAktarilanFon
  SDE:Toplam=SDE:BirikmisAmortismanFarki+SDE:BirikmisDegerlemeFonu+SDE:IlkAlimBedeli
  SDE:GidereAtilmayanAmortisman=0; Loop i#=1 To 75; SDE:GidereAtilmayanAmortisman+=Tah:BuYilGidereAtilmayanAmortisman[i#].;
Else
  SDE:SonNetDeger=0
  SDE:SonBrutDeger=0
  SDE:IlkAlimBedeli=0
  SDE:BirikmisAmortismanFarki=0
  SDE:BirikmisDegerlemeFonu=0
  SDE:Toplam=0
  SDE:GidereAtilmayanAmortisman=0
End
Display
