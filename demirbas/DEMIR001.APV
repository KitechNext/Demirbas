EMBED %WindowManagerMethodCodeSection
[INSTANCES]
WHEN 'Init'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 7800
PROPERTY:END
!If False
[SOURCE]
PROPERTY:BEGIN
PRIORITY 8001
PROPERTY:END
Set(Yasal); Next(Yasal)
If Toplu<>'E'
  Clear(Dem:Record); DEM:DemirbasKodu=OKodBas; Get(Demirbas,DEM:keykod)
  If ~DEM:Hesaplama
    If ErrorCode() Then Message(OKodBas&' Bulunam˝yor!.','Uyar˝ Ekran˝',ICON:Exclamation); Return 2.
    If DEM:DovizTutar>0 Then
      Do SatisKontrolu
      If ~DEM:KalanMiktar>0 Then Return 2.
      Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Get(Tahakkuk,Tah:keykod)
      If ErrorCode() Then Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Add(Tahakkuk).
      Line1=DEM:DemirbasKodu&' '&Clip(DEM:DemirbasAdi); Display
      No=(Yil-DEM:DonemYili)+1
      If Yil>=DEM:DonemYili And No<=50 Then If Yil=DEM:DonemYili Then Do BirinciYilHesap Else Do Hesapla..
      Do SatisKontroluIptal
    End
  End
Else
  Clear(Dem:Record);DEM:DemirbasKodu=OKodBas; Set(Dem:keykod,Dem:keykod)
  Loop
    Next(Demirbas); If ErrorCode() Then Break.
    If ~(DEM:DemirbasKodu>=OKodBas And (DEM:DemirbasKodu<=OKodSon Or OKodSon='')) Then Break.
    If ~(DEM:DemirbasGrubu>=BGrp And (DEM:DemirbasGrubu<=SGrp Or SGrp='')) Then Cycle.
    Grb:GrupKodu=DEM:DemirbasGrubu; Access:Grub.Fetch(Grb:KeyKod)
    If ~(Grb:AmacKodu>=BAmc And (Grb:AmacKodu<=SAmc Or SAmc='')) Then Cycle.

    If DEM:Hesaplama Then Cycle.
    If ~DEM:DovizTutar>0 Then Cycle.
    Do SatisKontrolu; If ~DEM:KalanMiktar>0 Then Cycle.
    Line1=DEM:DemirbasKodu&' '&Clip(DEM:DemirbasAdi); Display
    No=(Yil-Dem:DonemYili)+1
    If Yil>=Dem:DonemYili And No<=50
      Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Get(Tahakkuk,Tah:keykod)
      If ErrorCode() Then Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Add(Tahakkuk).
      If Yil=Dem:DonemYili Then Do BirinciYilHesap Else Do Hesapla.
    End
    Do SatisKontroluIptal
  End
End
Return 2
[END]
[END]
[END]
EMBED %ProcedureRoutines
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 2916
PROPERTY:END
SatisKontrolu Routine
      If Yil>=DEM:DonemYili   ! Bu Yil «˝k˝˛ Miktar˝
        SDE:DemirbasKodu=DEM:DemirbasKodu;SDE:SatisYil='0000'; Set(SDE:keykod,SDE:keykod); Adet=0
        Loop
          Next(SatDemir); If ErrorCode() Then Break.
          If ~(SDE:DemirbasKodu=DEM:DemirbasKodu) Then Break.
          If Yil>=SDE:SatisYil Then Adet+=SDE:SatisMiktar Else Break.
        End
        DEM:KalanMiktar=DEM:Miktar; DEM:KalanMiktar-=Adet; Put(Demirbas);
      End


[SOURCE]
PROPERTY:BEGIN
PRIORITY 3240
PROPERTY:END
SatisKontroluIptal  Routine
      SDE:DemirbasKodu=DEM:DemirbasKodu; SDE:SatisYil='0000'; Set(SDE:keykod,SDE:keykod); Adet=0
      Loop
        Next(SatDemir); If ErrorCode() Then Break.
        If ~(SDE:DemirbasKodu=DEM:DemirbasKodu) Then Break.
        Adet+=SDE:SatisMiktar
      End
      DEM:KalanMiktar=DEM:Miktar; DEM:KalanMiktar-=Adet; Put(Demirbas);
[SOURCE]
PROPERTY:BEGIN
PRIORITY 3600
PROPERTY:END
Hesapla Routine
  If Yil > Dem:DonemYili
    No=(Yil-Dem:DonemYili)+1

    If ~Tah:DovizBuYilAyrilanAmortisman[No-1]>0
        If Toplu<>'E' Then Message(Tah:DemirbasKodu&' Dˆviz ÷nceki Dˆnem Hesaplanmal˝d˝r.!',|
                                   'Uyar˝ Ekran˝',ICON:Exclamation).
    Else
      If Tah:KalanDovizDeger[No-1]=0 Then
          If Toplu<>'E' Then Message(Tah:DemirbasKodu&' Dˆviz ÷zel Maliyet S˝f˝rlanm˝˛t˝r.!',|
                                     'Uyar˝ Ekran˝',ICON:Exclamation).
      Else

        If DEM:Miktar<>DEM:KalanMiktar Then Dov$=Round((DEM:DovizTutar/DEM:Miktar)*DEM:KalanMiktar,.01)
        Else Dov$=DEM:DovizTutar.
        If DEM:AmortismanEritmeYili=No ! Son Yil
          If Ay=12 Then Tah:DovizBuYilAyrilanAmortisman[No]=Dov$-Tah:DovizBuYilBirikmisAmortisman[No-1]
          Else Tah:DovizBuYilAyrilanAmortisman[No]=Round(((Dov$-Tah:DovizBuYilBirikmisAmortisman[No-1])/12)*Ay,.01).

          If DEM:Miktar<>DEM:KalanMiktar Then
            Tah:DovizBuYilBirikmisAmortisman[No]=Round((Tah:DovizBuYilBirikmisAmortisman[No-1]/Dem:Miktar)*Dem:KalanMiktar,.01)
          Else Tah:DovizBuYilBirikmisAmortisman[No]=Tah:DovizBuYilBirikmisAmortisman[No-1].

          !Tah:DovizBuYilBirikmisAmortisman[No]=Tah:DovizBuYilBirikmisAmortisman[No-1]
          If DEM:Miktar<>DEM:KalanMiktar
            Tah:DovizBuYilBirikmisAmortisman[No]=Round((Tah:DovizBuYilBirikmisAmortisman[No-1]/DEM:Miktar)*DEM:KalanMiktar,.01)+|
                                                 Tah:DovizBuYilAyrilanAmortisman[No]
