[PROCEDURE]
NAME Crc16
PROTOTYPE '(string),ULong'
PARAMETERS '(Array)'
[COMMON]
FROM ABC Source
[DATA]
[SCREENCONTROLS]
! PROMPT('Crc:'),USE(?Crc:Prompt)
! ENTRY(@n13),USE(Crc)
[REPORTCONTROLS]
! STRING(@n13),USE(Crc)
Crc                      ULONG
!!> GUID('c4a02154-525d-44c6-ba20-7fc5c06c180a'),PROMPT('Crc:'),HEADER('Crc'),PICTURE(@n13)
[PROMPTS]
%GenerateOpenClose LONG  (0)
%GenerateSaveRestore LONG  (0)
[EMBED]
EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  Loop X#=1 to Len(Clip(Array))
    Crc=Bor(Crc,Array[x#])
    Loop 8 Times
      Crc=Bshift(Crc,1)
      If Band(Crc,1000000h)
        Crc=Bxor(Crc,102100h)
      End
    End
  End
  Return(Band(Bshift(Crc,-8),0000ffffh))
[END]
[END]
