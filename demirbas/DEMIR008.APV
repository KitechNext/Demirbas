[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
   NCB.ncb_command = NCBRESET
   I# = Netbios(NCB)

   NCB.ncb_callname = '*               '
   NCB.ncb_command = NCBASTAT

   NCB.ncb_lana_num = 0
   NCB.ncb_length = Size(AST)
   dwFlags = BOR(HEAP_GENERATE_EXCEPTIONS,HEAP_ZERO_MEMORY)

   pASTAT = HeapAlloc(GetProcessHeap(),dwFlags, NCB.ncb_length)

   If pASTAT = 0 Then Message('Hafýza Ayýrma Hatasý!'); Return ''.

   NCB.ncb_buffer = pASTAT
   I#=Netbios(NCB)

   Memcpy(Address(AST), NCB.ncb_buffer, Size(AST)); Tmp='DB-'
   BStr=AST.adapt.adapter_address[1]; Do ByteToHex1; tmp=Clip(Tmp)&Clip(Str)
   BStr=AST.adapt.adapter_address[2]; Do ByteToHex1; tmp=Clip(Tmp)&Clip(Str)&'-'
   BStr=AST.adapt.adapter_address[3]; Do ByteToHex1; tmp=Clip(Tmp)&Clip(Str)
   BStr=AST.adapt.adapter_address[4]; Do ByteToHex1; tmp=Clip(Tmp)&Clip(Str)&'-'
   BStr=AST.adapt.adapter_address[5]; Do ByteToHex1; tmp=Clip(Tmp)&Clip(Str)
   BStr=AST.adapt.adapter_address[6]; Do ByteToHex1; tmp=Clip(Tmp)&Clip(Str)
   HeapFree( GetProcessHeap(), 0, pASTAT)

   Tmp=Clip(Tmp)&'-'&NumtoHex(Volume())
   Return tmp

ByteToHex1 Routine
  Str = '00'
  EXECUTE BStr/16
    Str[1]='1'; Str[1]='2'; Str[1]='3'; Str[1]='4'; Str[1]='5'; Str[1]='6'; Str[1]='7'; Str[1]='8'
    Str[1]='9'; Str[1]='A'; Str[1]='B'; Str[1]='C'; Str[1]='D'; Str[1]='E'; Str[1]='F'
  END
  EXECUTE BStr % 16
    Str[2]='1'; Str[2]='2'; Str[2]='3'; Str[2]='4'; Str[2]='5'; Str[2]='6'; Str[2]='7'; Str[2]='8'
    Str[2]='9'; Str[2]='A'; Str[2]='B'; Str[2]='C'; Str[2]='D'; Str[2]='E'; Str[2]='F'
  END


