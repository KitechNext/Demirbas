[PROCEDURE]
NAME GetDiskSeriNo
PROTOTYPE '(STRING,*STRING),Byte'
PARAMETERS '(strDrive, strSerial)'
[COMMON]
FROM ABC Source
[PROMPTS]
%GenerateOpenClose LONG  (0)
%GenerateSaveRestore LONG  (0)
[EMBED]
EMBED %DataSection
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
! Ýnside the global Map
!MODULE('Windows API')
!  GetVolumeInformationA(*LPCSTR,*LPSTR,DWORD,*DWORD,*DWORD,*DWORD,*LPSTR,DWORD), BOOL,PASCAL,RAW
!  SysUlong2Hex(
!END




SerialNumber            STRING(10)
RootPathName            CSTRING(20)
VolumeNameBuffer        CSTRING(20)
VolumeNameSize          ULONG(18)
VolumeSerialNumber      ULONG
MaximumComponentLength  ULONG
FileSystemFlags         ULONG
FileSystemNameBuffer    CSTRING(20)
FileSystemNameSize      ULONG(18)
Ret                     ULONG
VSN                     &ULONG
MCL                     &ULONG
FSF                     &ULONG
[END]
EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  RootPathName        = UPPER(strDrive) & ':\'

  VSN                &= VolumeSerialNumber
  MCL                &= MaximumComponentLength
  FSF                &= FileSystemFlags

  Ret                 = GetVolumeInformationA( |
                        RootPathName,         |
                        VolumeNameBuffer,     |
                        VolumeNameSize,       |
                        VSN,                  |
                        MCL,                  |
                        FSF,                  |
                        FileSystemNameBuffer, |
                        FileSystemNameSize)

  !SerialNumber        = sysULong2Hex(VolumeSerialNumber)
  !SerialNumber        = SUB(SerialNumber,1,4) & '-' & SUB(SerialNumber,5,4)
  SerialNumber        = VolumeSerialNumber
  strSerial           = SerialNumber
  !strLabel            = VolumeNameBuffer
  !strFileType         = FileSystemNameBuffer
  RETURN(True)
