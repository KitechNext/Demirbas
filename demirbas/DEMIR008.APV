EMBED %ControlEventHandling
[INSTANCES]
WHEN '?OkButton'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4499
PROPERTY:END
  If UserId ='' Then Select(?USR:Id); Cycle.
  If USR:Id='' Then Select(?USR:Id); Cycle.
  If SifDeg
    If Sifre1='' Or Sifre2='' Then Select(?Sifre1); Display; Cycle.
    If Sifre1=Sifre2 Then Usr:Password=Upper(Sifre1); Put(KulUsers); LocalResponse=1; Break
    Else !Þifre Yanlýþ Tekrar Edildi.
      Message('Yeni Girilen Þifre Yanlýþ Tekrar Edildi'); Sifre1='';Sifre2='';Select(?Sifre1); Display.
  Else
    If Upper(Loc:Password)=Upper(Usr:Password) Then LocalResponse=1; MasterUser=Usr:MasterUser; Break
    ElsIf Attempts=3; LocalResponse=0; Break
    Else Attempts+=1; Message('  Lütfen Þifreyi Yeniden Giriniz.',,ICON:Hand); Select(?Loc:Password);Display.
  End

[END]
[END]
WHEN '?CancelButton'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4499
PROPERTY:END
LocalResponse=0
Break
[END]
[END]
WHEN '?USR:Id'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
If Usr:Id='' Then Select(?Usr:Id); Cycle
Else Get(KulUsers,Usr:IdKey); If ErrorCode() Then Message('Geçersiz Kullanýcý'); Select(?Usr:Id); Cycle..
IniMgr.Update('User','Name',Usr:Id); UserId=Usr:Id
[END]
[END]
