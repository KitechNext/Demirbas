      If ~(DEM:DemirbasKodu>=DKodBas And (DEM:DemirbasKodu<=DKodSon Or DKodSon='')) Then Break.
      If ~(DEM:KalanMiktar>0) Then Cycle.
      If ~(Yil<Year(DEM:AlimTarih)) ! Yýl Alým Trh Kucuk Degilse
         No=(Yil-Year(DEM:AlimTarih))+1
         Line1='Toplam ... '&DEM:DemirbasKodu&' '&Clip(DEM:DemirbasAdi); Display
         !Tahakkuk Dosyasý Baglantýsý
         Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Get(Tahakkuk,Tah:keykod)
         If ErrorCode() Then Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Add(Tahakkuk); Break.
         Loop i#=1 To No; Toplam$+=Tah:DegerlemeFonu[i#].
      End
    End !Loop
    !OranBul

    If SerAktTutar=0 Then SerAktTutar=Toplam$.

    If Toplam$<SerAktTutar Then Message('Sermayeye Aktarýlacak Fon Toplamý<10>'&Format(Toplam$,@n18)&' Kadar Olabilir'); Return 2.
    Oran$=SerAktTutar/Toplam$

!    Message('Oran='&Format(Oran$,@n6.2)&' Tutar='&Format(Toplam$,@n18.2))
    !Uygula
    Clear(DEM:Record);DEM:DemirbasKodu=DKodBas; Set(DEM:keykod,DEM:keykod); Toplam$=0
    Loop
      Next(Demirbas); If ErrorCode() Then Break.
      If ~(DEM:DemirbasKodu>=DKodBas And (DEM:DemirbasKodu<=DKodSon Or DKodSon='')) Then Break.
      If ~(DEM:KalanMiktar>0) Then Cycle.

      If ~(Yil<Year(DEM:AlimTarih)) ! Yýl Alým Trh Kucuk Degilse
         No=(Yil-Year(DEM:AlimTarih))+1
         Line1=DEM:DemirbasKodu&' '&Clip(DEM:DemirbasAdi); Display
         !Tahakkuk Dosyasý Baglantýsý
         Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Get(Tahakkuk,Tah:keykod)
         If ErrorCode() Then Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Add(Tahakkuk); Break.
         DEM:SermayeAktarilanFon=0;  Loop i#=1 To No; DEM:SermayeAktarilanFon+=Tah:DegerlemeFonu[i#]*Oran$.
         !DEM:SermayeAktarilanFon=Round(DEM:SermayeAktarilanFon,.01);
         DEM:SermayeAktarimTarihi=Yil; DEM:BirikmisDegerlemeFonu-=DEM:SermayeAktarilanFon; Put(Demirbas)
         Toplam$+=DEM:SermayeAktarilanFon
      End
    End !Loop

    !Balans
    Clear(DEM:Record);DEM:DemirbasKodu=DKodBas; Set(DEM:keykod,DEM:keykod);
    Loop
      Next(Demirbas); If ErrorCode() Then Break.
      If ~(DEM:DemirbasKodu>=DKodBas And (DEM:DemirbasKodu<=DKodSon Or DKodSon='')) Then Break.
      If ~(DEM:KalanMiktar>0) Then Cycle.

      If ~(Yil<Year(DEM:AlimTarih)) ! Yýl Alým Trh Kucuk Degilse
         If Toplam$<>SerAktTutar
           DEM:BirikmisDegerlemeFonu+=(Toplam$-SerAktTutar); DEM:SermayeAktarilanFon-=(Toplam$-SerAktTutar); Put(Demirbas)
         End
         Break
      End
    End


    Line1='Sermayeye Aktarým Ýþlemi Tamamlandý!'; Display
  End
End
[END]
