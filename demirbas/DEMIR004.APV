[EMBED]
EMBED %ControlEventHandling
[INSTANCES]
WHEN '?Button1'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
If Yil='' Then Select(?Yil); Display
Else
  If Message(Yil&' yýlý Tahakkuk Bilgileri silinecek. Emin misiniz?',|
            'Toplu Tahakkuk Silme',Icon:Question,BUTTON:YES+BUTTON:NO,BUTTON:NO,0)=BUTTON:YES

    !Sermayeye Aktarým Varmý Kontrol Ediliyor..
    Clear(DEM:Record); DEM:DemirbasKodu=DKodBas; Set(DEM:keykod,DEM:keykod); Ok#=0
    Loop Until Access:Demirbas.Next()

      If ~(DEM:DemirbasKodu>=DKodBas And (DEM:DemirbasKodu<=DKodSon Or DKodSon='')) Then Cycle.
      If ~(DEM:DemirbasGrubu>=BGrp And (DEM:DemirbasGrubu<=SGrp Or SGrp='')) Then Cycle.
      Clear(Grb:Record); Grb:GrupKodu=DEM:DemirbasGrubu; Access:Grub.Fetch(Grb:KeyKod)
      If ~(Grb:AmacKodu>=BAmc And (Grb:AmacKodu<=SAmc Or SAmc='')) Then Cycle.

      If DEM:Hesaplama Then Cycle.
      If (DEM:SermayeAktarimTarihi>=Yil) Then Ok#=1; Break.
    End
    If Ok# Then Message(DEM:DemirbasKodu&' '&DEM:SermayeAktarimTarihi&' '&Yil)
    Message(Yil&' yýlý itibari ile sermayeye aktarýlan bilgi mevcuttur.|Öncelikle sermayeye aktarýmý iptal ediniz..','Uyarý Ekraný',Icon:Exclamation); Cycle.

    Clear(DEM:Record); DEM:DemirbasKodu=DKodBas; Set(DEM:keykod,DEM:keykod)
    Loop Until Access:Demirbas.Next()
      If ~(DEM:DemirbasKodu>=DKodBas And (DEM:DemirbasKodu<=DKodSon Or DKodSon='')) Then Cycle.
      If ~(DEM:DemirbasGrubu>=BGrp And (DEM:DemirbasGrubu<=SGrp Or SGrp='')) Then Cycle.

      Clear(Grb:Record); Grb:GrupKodu=DEM:DemirbasGrubu; Access:Grub.Fetch(Grb:KeyKod)
      If ~(Grb:AmacKodu>=BAmc And (Grb:AmacKodu<=SAmc Or SAmc='')) Then Cycle.

      If DEM:Hesaplama Then Cycle.
      If DEM:AlimTarih='' Then Cycle.

      If ~(Yil<DEM:DonemYili) ! Yýl Alým Trh Kucuk Degilse
        !If ~(Yil>(DEM:DonemYili+DEM:AmortismanEritmeYili)) ! Yil Eritme Yilindan Buyuk Degilse
          No=(Yil-DEM:DonemYili)+1
          Line1=DEM:DemirbasKodu&' '&Clip(DEM:DemirbasAdi); Display

          Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Get(Tahakkuk,Tah:keykod)
          If ErrorCode() Then Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Add(Tahakkuk); Cycle.
          If (Tah:YDSSabitKiymetDegeri[No+1]+Tah:YDOSabitKiymetDegeri[No+1]+Tah:YDOAmortismanDegeri[No+1]+|
             Tah:YDSAmortismanDegeri[No+1]+Tah:AmortismanFarki[No+1]+Tah:EskiNetDeger[No+1]+|
             Tah:YeniNetDeger[No+1]+Tah:DegerlemeFonu[No+1]+Tah:SabitKiymetNetArtisi[No+1]+|
             Tah:BuYilAyrilanAmortisman[No+1]+Tah:BuYilBirikmisAmortisman[No+1]+Tah:KalanNetDeger[No+1])=0
            Tah:YDOSabitKiymetDegeri[No]=0
            Tah:YDOAmortismanDegeri[No]=0
            Tah:YDSSabitKiymetDegeri[No]=0
            Tah:YDSAmortismanDegeri[No]=0
            Tah:AmortismanFarki[No]=0
            Tah:EskiNetDeger[No]=0
            Tah:YeniNetDeger[No]=0
            Tah:DegerlemeFonu[No]=0
            Tah:SabitKiymetNetArtisi[No]=0
            Tah:BuYilAyrilanAmortisman[No]=0
            Tah:BuYilBirikmisAmortisman[No]=0
            Tah:KalanNetDeger[No]=0
            Tah:DovizBuYilAyrilanAmortisman[No]=0
            Tah:DovizBuYilBirikmisAmortisman[No]=0
            Tah:KalanDovizDeger[No]=0
            Tah:KistSonYilGidereAtilan[No]=0
            Tah:BuYilGidereAtilmayanAmortisman[No]=0
            Put(Tahakkuk)
            DEM:BirikmisDegerlemeFonu=0; Loop i#=1 To 50; DEM:BirikmisDegerlemeFonu+=Tah:DegerlemeFonu[i#].
            DEM:BirikmisDegerlemeFonu-=DEM:SermayeAktarilanFon; Put(Demirbas)
          End
        !End
      End
    End !Loop
    Line1='Silme Ýþlemi Tamamlandý!'; Display
  End
End
