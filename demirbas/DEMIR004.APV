[EMBED]
EMBED %BeforePrint
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  If YilYaz=0 Then YilYaz=Year(DEM:AlimTarih)&'/'&Format(Month(DEM:AlimTarih),@n02).
  Yil=(DemYil-DEM:DonemYili)+1
  If (AmorSec=DEM:HesapSekli Or AmorSec='T') And (Yil>=1 And Yil<75) And InRange(DEM:AlimTarih,BTrh,Strh) Then

     If ~YilYaz=Year(DEM:AlimTarih)&'/'&Format(Month(DEM:AlimTarih),@n02) Then !Tarih Toplamý Yazýlýyor...
       Loop Q#=1 To Records(Rapq)
         Get(RapQ,Q#); If Error() Then Break.
         If Rpq:Ntlk='D' And Rpq:deger[2]>0 Then
           If ~Excel Then Satir=Clip(DipSatir1); Do YazKontrol
           Else
             OleObj{'Application.WorkBooks("'&TWrkbName&'").WorkSheets("'&TShtName&'").Cells('&XRow&','&1&').EntireRow.Font.Bold'}=True
             OleObj{'Application.WorkBooks("'&TWrkbName&'").WorkSheets("'&TShtName&'").Cells('&XRow&','&1&').EntireRow.Font.ColorIndex'}=3
           End
           RapDiz('D',Satir,2); Satir[1:15]=YilYaz&' Toplamý'; Do YazKontrol; Satir=''; Do YazKontrol; Break
         End
       End
       YilYaz=Year(DEM:AlimTarih)&'/'&Format(Month(DEM:AlimTarih),@n02)
     End

      ! Tahakkuk Dosyasýna Baglanýyoruz
      Clear(Tah:Record); Tah:DemirbasKodu=DEM:DemirbasKodu; Get(Tahakkuk,Tah:keykod); If ErrorCode() Then Clear(Tah:Record).
      Clear(OTah:Record); OTah:DemirbasKodu=DEM:DemirbasKodu; Get(OTahakkuk,OTah:keykod); If ErrorCode() Then Clear(OTah:Record).
      If Itfa Then If ~(Tah:BuYilAyrilanAmortisman[Yil]>0) Then Return 2..

      Clear(Sde:Record); SDE:DemirbasKodu=DEM:DemirbasKodu; SDE:SatisYil=DemYil; Get(SatDemir,SDE:keykod); If ErrorCode() Then Clear(Sde:Record).
      If (Sat='H' And SDE:SatisMiktar<=0) Then Return 2.

      Bind('Yil',Yil);Bind('DemYil',DemYil);Bind('YDAF',YDAF); Bind('ILKDEGER',ILKDEGER);  Bind('Katsayi',Katsayi);
      Bind('KalanMiktar',KalanMiktar);  Do SatisKontrolu

      Clear(Deg:Record); DEG:DegerlemeYili=Year(DEM:AlimTarih); Access:Degerleme.Fetch(DEG:KeyYil)
      KatSayi=DEG:KatsayiOrani[Month(DEM:AlimTarih)]

      !Birikmiþ YDAF Hesabý
      YDAF=0; Loop ii#=1 To Yil ;
        Clear(Sde:Record); SDE:DemirbasKodu=DEM:DemirbasKodu; SDE:SatisYil=(ii#+DEM:DonemYili)-1; SYDAF$=0
        Set(SDE:keykod,SDE:keykod)
        Loop Until Access:SatDemir.Next()
          If ~(SDE:DemirbasKodu=DEM:DemirbasKodu And SDE:SatisYil=(ii#+DEM:DonemYili)-1) Then Break.
          SYDAF$+=SDE:BirikmisDegerlemeFonu
        End
        YDAF+=(Tah:DegerlemeFonu[ii#]-SYDAF$)
      End
      Clear(Sde:Record); SDE:DemirbasKodu=DEM:DemirbasKodu; SDE:SatisYil=DemYil; Get(SatDemir,SDE:keykod); If ErrorCode() Then Clear(Sde:Record).

      If Rap:Filtre1<>'' Then Case Evaluate(Rap:Filtre1); Of '';Message('1. Filtre Yanlýþ'); Return 3; Of 0 ; Return 2..
      If Rap:Filtre2<>'' Then Case Evaluate(Rap:Filtre2); Of '';Message('2. Filtre Yanlýþ'); Return 3; Of 0 ; Return 2..
      If Rap:Filtre3<>'' Then Case Evaluate(Rap:Filtre3); Of '';Message('3. Filtre Yanlýþ'); Return 3; Of 0 ; Return 2..

      If DemYil>2004 ! 2005 öncesi TL lerin Ytl Çevirilmesi
        !Tahakkauk
        Loop SonYilBul#=Yil To 1 by -1; If Tah:BuYilBirikmisAmortisman[SonYilBul#]=0 Then Cycle Else Break..
        If ((DEM:DonemYili+SonYilBul#)-1)<2005 And SonYilBul#>0
          Tah:YDOSabitKiymetDegeri[SonYilBul#]=Round(Tah:YDOSabitKiymetDegeri[SonYilBul#]/1000000,.01)
          Tah:YDOAmortismanDegeri[SonYilBul#]=Round(Tah:YDOAmortismanDegeri[SonYilBul#]/1000000,.01)
          Tah:YDSSabitKiymetDegeri[SonYilBul#]=Round(Tah:YDSSabitKiymetDegeri[SonYilBul#]/1000000,.01)
          Tah:YDSAmortismanDegeri[SonYilBul#]=Round(Tah:YDSAmortismanDegeri[SonYilBul#]/1000000,.01)
          Tah:AmortismanFarki[SonYilBul#]=Round(Tah:AmortismanFarki[SonYilBul#]/1000000,.01)
          Tah:EskiNetDeger[SonYilBul#]=Round(Tah:EskiNetDeger[SonYilBul#]/1000000,.01)
          Tah:YeniNetDeger[SonYilBul#]=Round(Tah:YeniNetDeger[SonYilBul#]/1000000,.01)
          Tah:DegerlemeFonu[SonYilBul#]=Round(Tah:DegerlemeFonu[SonYilBul#]/1000000,.01)
          Tah:SabitKiymetNetArtisi[SonYilBul#]=Round(Tah:SabitKiymetNetArtisi[SonYilBul#]/1000000,.01)
          Tah:BuYilAyrilanAmortisman[SonYilBul#]=Round(Tah:BuYilAyrilanAmortisman[SonYilBul#]/1000000,.01)
          Tah:KalanNetDeger[SonYilBul#]=Round(Tah:KalanNetDeger[SonYilBul#]/1000000,.01)
          Tah:BuYilGidereAtilmayanAmortisman[SonYilBul#]=Round(Tah:BuYilGidereAtilmayanAmortisman[SonYilBul#]/1000000,.01)
          Tah:BuYilBirikmisAmortisman[SonYilBul#]=Round(Tah:BuYilBirikmisAmortisman[SonYilBul#]/1000000,.01)
        End
        Tah:DuzeltmeyeEsasTutar=Round(Tah:DuzeltmeyeEsasTutar/1000000,.01)
        Tah:DuzeltilmisBirikmisAmortisman=Round(Tah:DuzeltilmisBirikmisAmortisman/1000000,.01)
        Tah:DuzKistSonYilGidereAtilan=Round(Tah:DuzKistSonYilGidereAtilan/1000000,.01)
        Tah:BinaDuzDegerlenmemisBirAmort=Round(Tah:BinaDuzDegerlenmemisBirAmort/1000000,.01)
        If DEM:DonemYili<2005
          Tah:KistSonYilGidereAtilan[1]=Round(Tah:KistSonYilGidereAtilan[1]/1000000,.01)
        End
        !Demirbaþ
        If Year(DEM:AlimTarih)<2005
          DEM:BirimTutar=Round(DEM:BirimTutar/1000000,.01)
          DEM:Tutar=Round(DEM:Tutar/1000000,.01)
          DEM:BirikmisDegerlemeFonu=Round(DEM:BirikmisDegerlemeFonu/1000000,.01)
          DEM:SermayeAktarilanFon=Round(DEM:SermayeAktarilanFon/1000000,.01)
          DEM:EnflasyonaEsasBirimTutar=Round(DEM:EnflasyonaEsasBirimTutar/1000000,.01)

          SDE:IlkAlimBedeli=Round(SDE:IlkAlimBedeli/1000000,.01)
          SDE:BirikmisAmortismanFarki=Round(SDE:BirikmisAmortismanFarki/1000000,.01)
          SDE:BirikmisDegerlemeFonu=Round(SDE:BirikmisDegerlemeFonu/1000000,.01)
          SDE:Toplam=Round(SDE:Toplam/1000000,.01)
          SDE:GidereAtilmayanAmortisman=Round(SDE:GidereAtilmayanAmortisman/1000000,.01)
        End
      End

      Bind('ILKDEGER',ILKDEGER); Bind('YTLILKDEGER',YTLILKDEGER); IlkDeger=0; YtlIlkDeger=0;
      If DEM:DonemYili=DemYil Then IlkDeger=DEM:Tutar Else IlkDeger=Tah:YDOSabitKiymetDegeri[Yil].

      !Message('ILKDeger='&IlkDeger&' '&Tah:YDOAmortismanDegeri[Yil])
      If IlkDeger=0 !2004 Yýlýna Bak Deger Sýfýr Ýse Enf. Düz. Yaz.
        !Message('Geldim')
        Yl#=(2004-DEM:DonemYili)+1;
        If Tah:YDOSabitKiymetDegeri[Yl#]=0 Then
          IlkDeger=Round(Tah:DuzeltmeyeEsasTutar,.01);
          Tah:YDOAmortismanDegeri[Yil]=IlkDeger
          Tah:YDSSabitKiymetDegeri[Yil]=IlkDeger
          Tah:YDSAmortismanDegeri[Yil]=IlkDeger
          Tah:BuYilBirikmisAmortisman[Yil]=IlkDeger
        End
      End


      YtlIlkDeger=IlkDeger; If DemYil<2005 Then YtlIlkDeger=1.

      Bind('IlkAlimBedeli',IlkAlimBedeli); IlkAlimBedeli=DEM:Tutar

      Bind('BirikmisAmortisman2003',BirikmisAmortisman2003); Yl#=(2003-DEM:DonemYili)+1; BirikmisAmortisman2003=0
      If Yl#>0 Then BirikmisAmortisman2003=Tah:BuYilBirikmisAmortisman[yl#]
        If BirikmisAmortisman2003=0 Then
          Loop ik#=Yl# to 1 by -1; BirikmisAmortisman2003=Tah:BuYilBirikmisAmortisman[ik#]; If BirikmisAmortisman2003<>0 Then Break..
        End
        BirikmisAmortisman2003=Round(BirikmisAmortisman2003/1000000,.01)
      End

      Bind('BILANCODEGER',BILANCODEGER); BilancoDeger=0;
      If DEm:DonemYili=DemYil Then BilancoDeger=DEM:Tutar Else BilancoDeger=Tah:YDSSabitKiymetDegeri[Yil].

      If DEM:DonemYili+DEM:AmortismanEritmeYili<DemYil AND DEM:AmortismanAyirmaSekli<>'A' !Çekilen Yýlda Kalan Miktara Göre Kýst Uygula
        Loop ii#=1 To Yil-1; If ~(Tah:KalanNetDeger[ii#]=0) Then Cycle Else SonHspKlnMiktar#=Tah:Miktar[ii#]; Break..
        IF SonHspKlnMiktar#=0 Then SonHspKlnMiktar#=DEM:KalanMiktar.
        Tah:YDOSabitKiymetDegeri[ii#]=Round((Tah:YDOSabitKiymetDegeri[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:YDOAmortismanDegeri[ii#]=Round((Tah:YDOAmortismanDegeri[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:YDSSabitKiymetDegeri[ii#]=Round((Tah:YDSSabitKiymetDegeri[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:YDSAmortismanDegeri[ii#]=Round((Tah:YDSAmortismanDegeri[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:EskiNetDeger[ii#]=Round((Tah:EskiNetDeger[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:YeniNetDeger[ii#]=Round((Tah:YeniNetDeger[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:DegerlemeFonu[ii#]=Round((Tah:DegerlemeFonu[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:SabitKiymetNetArtisi[ii#]=Round((Tah:SabitKiymetNetArtisi[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:BuYilAyrilanAmortisman[ii#]=Round((Tah:BuYilAyrilanAmortisman[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:DovizBuYilAyrilanAmortisman[ii#]=Round((Tah:DovizBuYilAyrilanAmortisman[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:BuYilBirikmisAmortisman[ii#]=Round((Tah:BuYilBirikmisAmortisman[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:DovizBuYilBirikmisAmortisman[ii#]=Round((Tah:DovizBuYilBirikmisAmortisman[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:KalanNetDeger[ii#]=Round((Tah:KalanNetDeger[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:KalanDovizDeger[ii#]=Round((Tah:KalanDovizDeger[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
        Tah:KistSonYilGidereAtilan[ii#]=Round((Tah:KistSonYilGidereAtilan[ii#]/SonHspKlnMiktar#)*KalanMiktar,.01)
      End

      BIND('IzDeger',Loc:IzDeger); Loc:IzDeger=0
      Loop ii#=Yil To 1 By -1; If Tah:BuYilBirikmisAmortisman[ii#]=0 Then Cycle Else Loc:IzDeger=Tah:BuYilBirikmisAmortisman[ii#]-1; Break..

      ! Tanýmlanabilir Rapor Kolonlarý Bulunuyor.
      Q#=1; SatirToplam=0
      Loop  !1
        Get(RapQ,Q#); If Error() Then Break.; Q#+=1
        Case Rpq:Ntlk
          Of 'S'; Rpq:Sdeger=Evaluate(Rpq:Fieldkod)
          Of 'T'; Rpq:deger[1]=Evaluate(Rpq:FieldKod)
          Of 'D'; Rpq:deger[1]=Evaluate(Rpq:FieldKod)
            If DEM:Hesaplama
              Ok#=0; Loop ii#=Yil To 1 By -1; If (Tah:BuYilBirikmisAmortisman[ii#]=0) Then Cycle Else Ok#=ii#;Break..
              If ~Ok#=0 Then Yil#=Yil; Yil=Ok#; Rpq:deger[1]=Evaluate(Rpq:FieldKod); Yil=Yil#.
            Else
              Case Sat
              Of 'E' !Mevcut Demirbaþlar
                If Rpq:Deger[1]=0 And Rpq:SonYilYaz='E' Then
                  If DemYil=2003 And Tah:DuzeltmeyeEsasTutar>0 Then Rpq:deger[1]=Evaluate('Tah:DuzeltilmisBirikmisAmortisman')
                  Else
                    Loop ii#=Yil-1 To 1 by -1; If Tah:BuYilBirikmisAmortisman[ii#]=0 Then Cycle Else Break..
                    if ii#>0
                      If ~InString('DOVIZ',Upper(Rpq:FieldKod),1,1) Then Rpq:deger[1]=Evaluate('Tah:BuYilBirikmisAmortisman['&ii#&']')
                      Else Rpq:deger[1]=Evaluate('Tah:DovizBuYilBirikmisAmortisman['&ii#&']').
                    End
                  End
                End
              Of 'H'; !Satýlan Demirbaþlar
                 Ok#=0; Loop ii#=Yil To 1 By -1; If (Tah:BuYilBirikmisAmortisman[ii#]=0) Then Cycle Else Ok#=ii#; Break..
                 If ~Ok#=0 Then Yil#=Yil; Yil=Ok#;
                   Rpq:deger[1]=Evaluate(Rpq:FieldKod); Yil=Yil#.
              End
            End
          Of 'F'; i#=1; TdtSaha=''
            Loop !2
              If i#>Len(Clip(Rpq:Formul)) Then Break.
              If Rpq:Formul[i#]='$' Then ii#=Sub(Rpq:Formul,i#+1,2); X#=1
                Loop; Get(RapQ,X#); If ErrorCode() Then Break.; X#+=1; If Rpq:SiraNo<>ii# Then Cycle Else Break..
                If ErrorCode() Then TdtSaha=Clip(TdtSaha)&'0' Else TdtSaha=Clip(TdtSaha)&(Rpq:Deger[1]).
                i#+=3; Get(RapQ,Q#-1)
              Else TdtSaha=TdtSaha&Rpq:Formul[i#]; i#+=1.
            End !Loop 2
            Rpq:deger[1]=Evaluate(Clip(TdtSaha))
        End ! Case
        !If Sat='E' AND YtlIlkDeger=0 And DEM:DonemYili+DEM:AmortismanEritmeYili<=2005 And Rpq:Deger[1]>0 Then Rpq:Deger[1]/=1000000.

        ! Satir Filtre Kontrolu
        If Rpq:Filtre<>''
          Case Evaluate(Rpq:Filtre);Of '';Message('Filtre Yanlýþ'); Return 3
            Of 0 ; Case Rpq:Ntlk; Of 'S'; Rpq:Sdeger=''; Of 'D' ;OrOf 'F'; Rpq:Deger[1]=0..
        End
        SatirToplam+=Rpq:Deger[1]; Put(RapQ)
      End  ! Loop 1

      If GenelToplamAl=False Then GenelToplamAl=True.

      !Tanýmlanabilir Kolonlar Bulundu Satir Deðiþkenine diziliyor
      RapDiz('D',Satir,1); If DetayYaz Then Do YazKontrol Else Satir=''.; Display
  End
