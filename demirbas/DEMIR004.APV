!      Else XX$=DEM:EnflasyonaEsasBirimTutar*DEM:KalanMiktar
!         Loop i#=2 To 50; If ~Tah:YDSSabitKiymetDegeri[i#]=0 Then XX$=Tah:YDSSabitKiymetDegeri[i#] Else Break...

      If Year(DEM:AlimTarih)=Yil Then XX$=DEM:BirimTutar*DEM:KalanMiktar Else XX$=DEM:BirimTutar*DEM:KalanMiktar
         Loop i#=2 To 50; If ~Tah:YDSSabitKiymetDegeri[i#]=0 Then XX$=Tah:YDSSabitKiymetDegeri[i#] Else Break...

      YY$=0; Loop i#=1 To 50; If ~Tah:BuYilBirikmisAmortisman[i#]=0 Then YY$=Tah:BuYilBirikmisAmortisman[i#] Else Break..
      ArtisTutar$=Tah:DuzeltmeyeEsasTutar-XX$; ArtisOran$=Abs((ArtisTutar$/XX$));

      If ArtisTutar$>=0 Then Tah:DuzeltilmisBirikmisAmortisman=(YY$*ArtisOran$)+YY$
      Else Tah:DuzeltilmisBirikmisAmortisman=YY$-(YY$*ArtisOran$).

      If ArtisTutar$>=0 Then Tah:DuzKistSonYilGidereAtilan=(Tah:KistSonYilGidereAtilan[1]*ArtisOran$)+Tah:KistSonYilGidereAtilan[1]
      Else Tah:DuzKistSonYilGidereAtilan=Tah:KistSonYilGidereAtilan[1]-(Tah:KistSonYilGidereAtilan[1]*ArtisOran$).

      If DEM:HesapSekli='H' And DEM:AmortismanAyirmaSekli='B'
        !Bina için Düzeltme
        No=(Yil-DEM:DonemYili)+1; Tah:BinaDuzDegerlenmemisBirAmort=0;
        Loop i#=1 to No ;Tah:BinaDuzDegerlenmemisBirAmort+=Tah:BuYilAyrilanAmortisman[i#].;
!        Message(Tah:BinaDuzDegerlenmemisBirAmort&' '&ArtisOran$)
        If ArtisTutar$>=0 Then Tah:BinaDuzDegerlenmemisBirAmort=(Tah:BinaDuzDegerlenmemisBirAmort*ArtisOran$)+Tah:BinaDuzDegerlenmemisBirAmort
        Else Tah:BinaDuzDegerlenmemisBirAmort=Tah:BinaDuzDegerlenmemisBirAmort-(Tah:BinaDuzDegerlenmemisBirAmort*ArtisOran$).
!        Message(Tah:BinaDuzDegerlenmemisBirAmort)

      End
      Put(Tahakkuk)
    End
[END]
